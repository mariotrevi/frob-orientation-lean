      intro x hx; induction hx using Algebra.adjoin_induction ; aesop;
      · -- Since $N$ is prime, by Fermat's little theorem, we have $r^N \equiv r \pmod{N}$ for any integer $r$.
        have h_fermat : ∀ r : ℤ, r ^ N ≡ r [ZMOD N] := by
