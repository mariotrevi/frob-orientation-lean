lemma mem_C1_iff_chi_eq_s1_11779 (n : ZMod 11779) (hn : n ≠ 0) : n ∈ C1_11779 ↔ chi11779 n = s1_11779 := by
  -- We know C1 is the set of elements of the form g * x^3.
  -- chi(n) = n^((q-1)/3).
  -- chi(g * x^3) = (g * x^3)^((q-1)/3) = g^((q-1)/3) * (x^3)^((q-1)/3) = chi(g) * x^(q-1) = s1 * 1 = s1.
