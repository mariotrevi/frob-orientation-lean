    convert h_adjoin.adjoin_primitive_root_eq_top ( show IsPrimitiveRoot zeta_int7489 7489 from ?_ ) using 1;
    rw [ IsPrimitiveRoot.iff_def ] at *;
    convert h_primitive;
    · erw [ ← Subtype.coe_inj ] ; simp +decide [ zeta_int7489 ];
      exact?;
    · erw [ ← Subtype.coe_inj ] ; norm_num [ zeta_int7489 ];
