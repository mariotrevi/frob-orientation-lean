/-
The Artin property holds for all elements of the ring of integers.
-/
theorem artin_property_7489 (N : ℕ) [Fact (Nat.Prime N)] (hN : N ≠ 7489) (P : Ideal Ok7489) [P.IsPrime] (hP : P.LiesOver (Ideal.span {(N : ℤ)})) (x : Ok7489) :
  Ideal.Quotient.mk P (sigma_int7489 (N_mod_7489 N hN) x) = (Ideal.Quotient.mk P x)^N := by
    have h_sigma_pow : ∀ x : Ok7489, Ideal.Quotient.mk P (sigma_int7489 (N_mod_7489 N hN) x) = (Ideal.Quotient.mk P x)^N := by
      intro x
      have h_gen : x ∈ Algebra.adjoin ℤ {zeta_int7489} := by
        have h_gen : (⊤ : Subalgebra ℤ Ok7489) = Algebra.adjoin ℤ {zeta_int7489} := by
          exact?;
        exact h_gen ▸ Algebra.mem_top
      refine' Algebra.adjoin_induction _ _ _ _ h_gen <;> norm_num +zetaDelta at *;
      · convert artin_property_zeta_7489 N hN P hP using 1;
      · intro r; exact (by
        erw [ Ideal.Quotient.eq ] ; norm_num [ Ideal.mem_span_singleton ];
        have h_div : ↑N ∣ (r : ℤ) - r ^ N := by
          haveI := Fact.mk ( Fact.out : Nat.Prime N ) ; simp +decide [ ← ZMod.intCast_zmod_eq_zero_iff_dvd ] ;
        obtain ⟨ k, hk ⟩ := h_div;
        convert P.mul_mem_right ( k : Ok7489 ) ( hP.1 ▸ Ideal.mem_span_singleton_self ( N : ℤ ) ) using 1 ; norm_cast ; aesop);
      · intro x y hx hy hx' hy';
        have h_exp : (Ideal.Quotient.mk P) (sigma_int7489 (N_mod_7489 N hN) (x + y)) = (Ideal.Quotient.mk P) (sigma_int7489 (N_mod_7489 N hN) x + sigma_int7489 (N_mod_7489 N hN) y) := by
          exact congr_arg _ ( map_add _ _ _ );
        have h_exp : (Ideal.Quotient.mk P) ((x + y) ^ N) = (Ideal.Quotient.mk P) (x ^ N + y ^ N) := by
          have h_exp : (x + y) ^ N ≡ x ^ N + y ^ N [SMOD P] := by
            have h_exp : (x + y) ^ N = x ^ N + y ^ N + ∑ i ∈ Finset.Ico 1 N, Nat.choose N i * x ^ i * y ^ (N - i) := by
              rw [ add_pow ];
              rw [ Finset.sum_range_succ, Finset.sum_Ico_eq_sub _ ] <;> norm_num [ mul_assoc, mul_comm, mul_left_comm ];
              · ring;
              · exact Nat.Prime.pos Fact.out;
            have h_exp : ∀ i ∈ Finset.Ico 1 N, Nat.choose N i * x ^ i * y ^ (N - i) ∈ P := by
              intros i hi
              have h_choose : (Nat.choose N i : ℤ) * x ^ i * y ^ (N - i) ∈ P := by
                have h_choose : (Nat.choose N i : ℤ) ∈ Ideal.span {(N : ℤ)} := by
                  exact Ideal.mem_span_singleton.mpr ( mod_cast Nat.dvd_of_mod_eq_zero ( by rw [ Nat.mod_eq_zero_of_dvd ] ; exact Nat.Prime.dvd_choose_self ( Fact.out : Nat.Prime N ) ( by linarith [ Finset.mem_Ico.mp hi ] ) ( by linarith [ Finset.mem_Ico.mp hi ] ) ) )
                have h_choose : (Nat.choose N i : Ok7489) ∈ P := by
                  have h_choose : Ideal.span {(N : ℤ)} ≤ P.comap (algebraMap ℤ Ok7489) := by
                    exact hP.1.le
                  generalize_proofs at *; (
                  exact h_choose ‹_› |> fun h => by simpa using h;)
                generalize_proofs at *; (
                exact P.mul_mem_right _ ( P.mul_mem_right _ h_choose ))
              generalize_proofs at *; (
              convert h_choose using 1
              skip);
            rw [ SModEq ];
            rw [ Submodule.Quotient.eq ];
            exact ‹ ( x + y ) ^ N = x ^ N + y ^ N + ∑ i ∈ Finset.Ico 1 N, ↑ ( N.choose i ) * x ^ i * y ^ ( N - i ) › ▸ by simpa using P.sum_mem fun i hi => h_exp i hi;
          exact?;
        convert congr_arg₂ ( · + · ) hx' hy' using 1;
      · intro x y hx hy hx' hy'; rw [ mul_pow, hx', hy' ] ;
    exact h_sigma_pow x

