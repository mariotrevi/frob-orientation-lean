      exact Subtype.ext h_exp;
    rw [ ← Nat.mod_add_div ( ( n : ZMod 7489 ).val * x.val ) 7489, pow_add, pow_mul ] ; aesop

/-
If n is in H, then multiplication by n permutes H, C1, C2 trivially.
-/
lemma action_H_7489 (n : ZMod 7489) (hn : n ∈ H7489) :
  H7489.image (fun x => n * x) = H7489 ∧
  C1_7489.image (fun x => n * x) = C1_7489 ∧
  C2_7489.image (fun x => n * x) = C2_7489 := by
    refine' ⟨ Finset.eq_of_subset_of_card_le ( Finset.image_subset_iff.mpr _ ) _, Finset.eq_of_subset_of_card_le ( Finset.image_subset_iff.mpr _ ) _, Finset.eq_of_subset_of_card_le ( Finset.image_subset_iff.mpr _ ) _ ⟩;
    all_goals push_cast [ H7489, C1_7489, C2_7489 ] at *;
    all_goals push_cast [ Finset.mem_image, Finset.mem_filter ] at *;
    all_goals rcases hn with ⟨ a, ⟨ ha₁, ha₂ ⟩, rfl ⟩;
    all_goals norm_num [ Finset.card_image_of_injective, Function.Injective, ha₂ ];
    · intro b hb;
      exact ⟨ a * b, mul_ne_zero ha₂ hb, by ring ⟩;
    · intro b hb; use a * b; simp +decide [ *, mul_comm, mul_assoc, mul_left_comm ] ;
      ring;
    · intro b hb; use a * b; simp +decide [ *, mul_pow, mul_assoc, mul_comm, mul_left_comm ] ;

/-
If n is in C1, then multiplication by n permutes H -> C1 -> C2 -> H.
-/
lemma action_C1_7489 (n : ZMod 7489) (hn : n ∈ C1_7489) :
  H7489.image (fun x => n * x) = C1_7489 ∧
  C1_7489.image (fun x => n * x) = C2_7489 ∧
  C2_7489.image (fun x => n * x) = H7489 := by
    norm_num [ C1_7489, C2_7489, H7489 ] at *;
    rcases hn with ⟨ a, ha, rfl ⟩;
    refine' ⟨ _, _, _ ⟩ <;> ext x <;> simp +contextual [ Finset.mem_image ];
    · constructor;
      · rintro ⟨ y, hy, rfl ⟩;
        exact ⟨ a * y, mul_ne_zero ha hy, by ring ⟩;
      · rintro ⟨ b, hb, rfl ⟩;
        -- Since $a \neq 0$ and $b \neq 0$, we can find $a_1$ such that $a_1^3 = b^3 / a^3$.
        obtain ⟨a_1, ha_1⟩ : ∃ a_1 : ZMod 7489, a_1^3 = b^3 / a^3 := by
          haveI := Fact.mk ( by norm_num : Nat.Prime 7489 ) ; exact ⟨ b / a, by ring ⟩ ;
        grind;
    · constructor <;> rintro ⟨ b, hb, rfl ⟩;
      · exact ⟨ a * b, mul_ne_zero ha hb, by ring ⟩;
      · -- Let $a_1 = b / a$.
        use b / a;
        grind;
    · constructor <;> intro h;
      · obtain ⟨ b, hb, rfl ⟩ := h;
        refine' ⟨ a * b * g7489, _, _ ⟩ <;> ring_nf at * ; simp_all +decide;
      · obtain ⟨ b, hb, rfl ⟩ := h;
        -- We need to find $a_1$ such that $g7489 * a^3 * (g7489^2 * a_1^3) = b^3$.
        -- This simplifies to $g7489^3 * a^3 * a_1^3 = b^3$, or $(g7489 * a * a_1)^3 = b^3$.
        -- Taking the cube root of both sides, we get $g7489 * a * a_1 = b$.
        suffices h_cube_root : ∃ a_1 : ZMod 7489, g7489 * a * a_1 = b by
          obtain ⟨ a_1, rfl ⟩ := h_cube_root;
          exact ⟨ a_1, by aesop_cat, by ring ⟩;
        have h_inv : ∃ a_1 : ZMod 7489, g7489 * a * a_1 = 1 := by
          have h_inv : IsUnit (g7489 * a) := by
            haveI := Fact.mk ( by native_decide : Nat.Prime 7489 ) ; exact IsUnit.mul ( by native_decide ) ( IsUnit.mk0 _ ha ) ;
          exact h_inv.exists_right_inv;
        exact ⟨ h_inv.choose * b, by linear_combination' h_inv.choose_spec * b ⟩

/-
If n is in C2, then multiplication by n permutes H -> C2 -> C1 -> H.
-/
lemma action_C2_7489 (n : ZMod 7489) (hn : n ∈ C2_7489) :
  H7489.image (fun x => n * x) = C2_7489 ∧
  C1_7489.image (fun x => n * x) = H7489 ∧
  C2_7489.image (fun x => n * x) = C1_7489 := by
    -- Since $n \in C2_7489$, there exists $m \in H7489$ such that $n = g7489^2 * m$.
    obtain ⟨m, hm⟩ : ∃ m ∈ H7489, n = g7489^2 * m := by
      native_decide +revert;
    -- Since multiplication by $m$ (which is in $H7489$) permutes $H7489$, $C1_7489$, and $C2_7489$ trivially, we can simplify the images.
    have h_perm : Finset.image (fun x => m * x) H7489 = H7489 ∧ Finset.image (fun x => m * x) C1_7489 = C1_7489 ∧ Finset.image (fun x => m * x) C2_7489 = C2_7489 := by
      exact ⟨ action_H_7489 m hm.1 |>.1, action_H_7489 m hm.1 |>.2.1, action_H_7489 m hm.1 |>.2.2 ⟩;
    -- Since multiplication by $g7489^2$ permutes $H7489$, $C1_7489$, and $C2_7489$ cyclically, we can simplify the images.
    have h_perm_g : Finset.image (fun x => g7489^2 * x) H7489 = C2_7489 ∧ Finset.image (fun x => g7489^2 * x) C1_7489 = H7489 ∧ Finset.image (fun x => g7489^2 * x) C2_7489 = C1_7489 := by
      native_decide +revert;
    simp +decide only [hm.2, mul_comm, mul_left_comm];
    have h_perm_final : Finset.image (fun x => m * (x * g7489^2)) H7489 = Finset.image (fun x => g7489^2 * x) (Finset.image (fun x => m * x) H7489) ∧ Finset.image (fun x => m * (x * g7489^2)) C1_7489 = Finset.image (fun x => g7489^2 * x) (Finset.image (fun x => m * x) C1_7489) ∧ Finset.image (fun x => m * (x * g7489^2)) C2_7489 = Finset.image (fun x => g7489^2 * x) (Finset.image (fun x => m * x) C2_7489) := by
      simp +decide [ mul_assoc, mul_comm, mul_left_comm, Finset.ext_iff ];
    aesop

/-
The automorphism sigma_n permutes alpha, beta, gamma according to which coset n belongs to.
-/
lemma global_permutation_7489 (n : (ZMod 7489)ˣ) :
  ((n : ZMod 7489) ∈ C1_7489 → sigma_int7489 n alpha_int7489 = beta_int7489) ∧
  ((n : ZMod 7489) ∈ C2_7489 → sigma_int7489 n alpha_int7489 = gamma_int7489) ∧
  ((n : ZMod 7489) ∈ H7489 → sigma_int7489 n alpha_int7489 = alpha_int7489) := by
    refine' ⟨ _, _, _ ⟩;
    · intro hn
      have h_sigma_eta : sigma_int7489 n (eta0_int7489 - eta1_int7489) = eta1_int7489 - eta2_int7489 := by
        have h_sigma_eta : sigma_int7489 n eta0_int7489 = eta1_int7489 ∧ sigma_int7489 n eta1_int7489 = eta2_int7489 := by
          have := action_C1_7489 n.val hn;
          have h_sigma_eta : ∀ S : Finset (ZMod 7489), sigma_int7489 n (eta_int7489 S) = eta_int7489 (S.image (fun x => (n : ZMod 7489) * x)) := by
            exact?;
          exact ⟨ h_sigma_eta H7489 ▸ this.1 ▸ rfl, h_sigma_eta C1_7489 ▸ this.2.1 ▸ rfl ⟩;
        rw [ map_sub, h_sigma_eta.1, h_sigma_eta.2 ];
      unfold alpha_int7489 beta_int7489;
      unfold sign_7489;
      split_ifs <;> simp_all +decide [ mul_assoc ];
      · unfold alpha_prime_int7489 beta_prime_int7489 gamma_prime_int7489 at * ; aesop;
      · unfold alpha_prime_int7489 beta_prime_int7489 gamma_prime_int7489 at * ; aesop;
    · intro hn;
      unfold alpha_int7489 gamma_int7489;
      unfold alpha_prime_int7489 gamma_prime_int7489; simp +decide [ mul_assoc, map_mul ] ;
      unfold eta0_int7489 eta1_int7489 eta2_int7489; simp +decide [ sigma_eta_7489, hn ] ;
      rw [ action_C2_7489 _ hn |>.1, action_C2_7489 _ hn |>.2.1 ] ; norm_num;
    · unfold alpha_int7489;
      unfold alpha_prime_int7489;
      unfold eta0_int7489 eta1_int7489; simp +decide [ mul_sub, sub_mul, sigma_eta_7489 ] ;
      intro hn; rw [ action_H_7489 _ hn |>.1, action_H_7489 _ hn |>.2.1 ] ;

