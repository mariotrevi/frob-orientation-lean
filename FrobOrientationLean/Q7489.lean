      exact?;
    -- Since $N$ is coprime to $7489$, we have $zeta^{N \mod 7489} = zeta^N$ in the quotient ring.
    have h_exp : (zeta_int7489 : Ok7489) ^ (N_mod_7489 N hN : ZMod 7489).val = (zeta_int7489 : Ok7489) ^ N := by
      have h_exp : (zeta_int7489 : Ok7489) ^ 7489 = 1 := by
        have h_exp : zeta7489 ^ 7489 = 1 := by
          exact IsCyclotomicExtension.zeta_pow _ _ _;
        exact Subtype.ext h_exp;
      -- Since $N$ is coprime to $7489$, we have $N \mod 7489 = N$.
      have h_mod : (N_mod_7489 N hN : ZMod 7489).val = N % 7489 := by
        unfold N_mod_7489; aesop;
      rw [ h_mod, ← Nat.mod_add_div N 7489, pow_add, pow_mul ] ; aesop;
