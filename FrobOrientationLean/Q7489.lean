  let s2 := 5021
  let c (n : Nat) := (n : Nat) ^ 2496 % q
  let order := q - 1
  -- 7488 = 16 * 468 = 16 * 4 * 117 = 64 * 9 * 13 = 2^6 * 3^2 * 13
