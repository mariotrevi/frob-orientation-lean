/-
Define the generator g = 7 and the cubic residue sets H, C1, C2.
-/
def g8563 : ZMod 8563 := 7

def H8563 : Finset (ZMod 8563) := ((Finset.univ : Finset (ZMod 8563)).filter (fun x => x ≠ 0)).image (fun x => x^3)

def C1_8563 : Finset (ZMod 8563) := H8563.image (fun x => g8563 * x)

def C2_8563 : Finset (ZMod 8563) := H8563.image (fun x => g8563^2 * x)

/-
Define the periods eta0, eta1, eta2 and the differences alpha', beta', gamma' in K8563.
-/
open scoped BigOperators

noncomputable def eta8563 (S : Finset (ZMod 8563)) : K8563 := ∑ x ∈ S, zeta8563^(x.val)

noncomputable def eta0_8563 := eta8563 H8563
noncomputable def eta1_8563 := eta8563 C1_8563
noncomputable def eta2_8563 := eta8563 C2_8563

noncomputable def alpha_prime_8563 := eta0_8563 - eta1_8563
noncomputable def beta_prime_8563 := eta1_8563 - eta2_8563
noncomputable def gamma_prime_8563 := eta2_8563 - eta0_8563

/-
Define Ok8563 and state the irreducibility of the cyclotomic polynomial.
-/
abbrev Ok8563 := NumberField.RingOfIntegers K8563

theorem h_irr_8563 : Irreducible (Polynomial.cyclotomic 8563 ℚ) := by
  exact Polynomial.cyclotomic.irreducible_rat <| by norm_num;

/-
Define the Galois automorphisms sigma_n for K8563.
-/
noncomputable def sigma8563 (n : (ZMod 8563)ˣ) : K8563 ≃ₐ[ℚ] K8563 :=
  (IsCyclotomicExtension.autEquivPow K8563 h_irr_8563).symm n

/-
Define the integral element zeta_int8563.
-/
theorem zeta_isIntegral_8563 : IsIntegral ℤ zeta8563 := by
  -- The cyclotomic polynomial Φ_8563 is monic, so zeta8563 is a root of a monic polynomial with integer coefficients.
  have h_cyclotomic_monic : Polynomial.Monic (Polynomial.cyclotomic 8563 ℤ) := by
    exact Polynomial.cyclotomic.monic _ _;
  refine' ⟨ Polynomial.cyclotomic 8563 ℤ, h_cyclotomic_monic, _ ⟩;
  simp +decide [ Polynomial.eval₂_eq_eval_map ];
  convert Polynomial.isRoot_cyclotomic_iff.mpr _;
  · infer_instance;
  · exact ⟨ by norm_num ⟩;
  · convert IsCyclotomicExtension.zeta_spec 8563 ℚ K8563

def zeta_int8563 : Ok8563 := ⟨zeta8563, zeta_isIntegral_8563⟩

/-
Define integral automorphisms and periods/differences in Ok8563.
-/
noncomputable def sigma_int8563 (n : (ZMod 8563)ˣ) : Ok8563 ≃ₐ[ℤ] Ok8563 :=
  (NumberField.RingOfIntegers.mapAlgEquiv (sigma8563 n)).restrictScalars ℤ

noncomputable def eta_int8563 (S : Finset (ZMod 8563)) : Ok8563 := ∑ x ∈ S, zeta_int8563^(x.val)

noncomputable def eta0_int8563 := eta_int8563 H8563
noncomputable def eta1_int8563 := eta_int8563 C1_8563
noncomputable def eta2_int8563 := eta_int8563 C2_8563

noncomputable def alpha_prime_int8563 := eta0_int8563 - eta1_int8563
noncomputable def beta_prime_int8563 := eta1_int8563 - eta2_int8563
noncomputable def gamma_prime_int8563 := eta2_int8563 - eta0_int8563

/-
Define the sign convention and the final periods alpha, beta, gamma.
-/
open Classical

noncomputable def sign_8563 : ℤ :=
  if alpha_prime_int8563 * beta_prime_int8563 * gamma_prime_int8563 = (8563 : Ok8563) then 1 else -1

noncomputable def alpha_int8563 := (sign_8563 : Ok8563) * alpha_prime_int8563
noncomputable def beta_int8563 := (sign_8563 : Ok8563) * beta_prime_int8563
noncomputable def gamma_int8563 := (sign_8563 : Ok8563) * gamma_prime_int8563

noncomputable def alpha8563_field : K8563 := alpha_int8563
noncomputable def beta8563_field : K8563 := beta_int8563
noncomputable def gamma8563_field : K8563 := gamma_int8563

/-
Define the DreamAssumptions for q=8563, including the period equation and the linear relations for beta and gamma in terms of alpha.
-/
def DreamAssumptions_8563 : Prop :=
  alpha_int8563^3 = 8563 * alpha_int8563 + 8563 ∧
  alpha_int8563 * beta_int8563 * gamma_int8563 = 8563 ∧
  185 * beta_int8563 = 3 * alpha_int8563^2 - 97 * alpha_int8563 - 17126 ∧
  185 * gamma_int8563 = -3 * alpha_int8563^2 + 88 * alpha_int8563 + 17126

/-
Define N as a unit mod 8563 and state that sigma_n(zeta) = zeta^n.
-/
def N_mod_8563 (N : ℕ) [Fact (Nat.Prime N)] (hN : N ≠ 8563) : (ZMod 8563)ˣ :=
  ZMod.unitOfCoprime N (Nat.Coprime.symm ((Nat.coprime_primes (by norm_num) (Fact.out)).mpr hN.symm))

lemma sigma_int_zeta_pow_8563 (n : (ZMod 8563)ˣ) :
  sigma_int8563 n zeta_int8563 = zeta_int8563 ^ (n : ZMod 8563).val := by
    convert artin_property_zeta_1567 using 1;
    constructor;
    · exact?;
    · unfold sigma_int8563;
      unfold sigma8563;
      erw [ Subtype.mk_eq_mk ] ; norm_num;
      erw [ PowerBasis.equivOfMinpoly_gen ];
      unfold IsPrimitiveRoot.powerBasis; aesop;

/-
State the Artin property for zeta: sigma_N(zeta) = zeta^N mod P.
-/
theorem artin_property_zeta_8563 (N : ℕ) [Fact (Nat.Prime N)] (hN : N ≠ 8563) (P : Ideal Ok8563) [P.IsPrime] (hP : P.LiesOver (Ideal.span {(N : ℤ)})) :
  Ideal.Quotient.mk P (sigma_int8563 (N_mod_8563 N hN) zeta_int8563) = (Ideal.Quotient.mk P zeta_int8563)^N := by
    have h_sigma_zeta : ∀ (n : (ZMod 8563)ˣ), (Ideal.Quotient.mk P (sigma_int8563 n zeta_int8563)) = (Ideal.Quotient.mk P zeta_int8563) ^ (n : ZMod 8563).val := by
      intro n
      have h_sigma_zeta : (Ideal.Quotient.mk P (sigma_int8563 n zeta_int8563)) = (Ideal.Quotient.mk P zeta_int8563 ^ (n : ZMod 8563).val) := by
        have := sigma_int_zeta_pow_8563 n
        rw [ this, map_pow ]
      exact h_sigma_zeta;
    rw [ h_sigma_zeta ];
    -- Since $P$ lies over $(N)$, we have $zeta8563^N = zeta8563^{N \mod 8563}$ in the quotient ring.
    have h_cong : (Ideal.Quotient.mk P zeta_int8563) ^ N = (Ideal.Quotient.mk P zeta_int8563) ^ (N % 8563) := by
      have h_cong : (Ideal.Quotient.mk P zeta_int8563) ^ 8563 = 1 := by
        have h_exp : zeta_int8563 ^ 8563 = 1 := by
          have h_exp : zeta8563 ^ 8563 = 1 := by
            exact IsCyclotomicExtension.zeta_pow _ _ _;
          exact Subtype.ext h_exp;
        simpa using congr_arg ( Ideal.Quotient.mk P ) h_exp;
      rw [ ← Nat.mod_add_div N 8563, pow_add, pow_mul ] ; aesop;
    exact?

/-
Define chi(x) = x^2854 and state that it is multiplicative.
-/
def chi8563 (x : ZMod 8563) : ZMod 8563 := x ^ 2854

lemma chi8563_mul (x y : ZMod 8563) : chi8563 (x * y) = chi8563 x * chi8563 y := by
  by_cases hx : x = 0 <;> by_cases hy : y = 0 <;> simp +decide [ hx, hy, pow_add, pow_mul, mul_pow, chi8563 ]

/-
State that zeta_int8563 is a primitive 8563-th root of unity.
-/
theorem zeta_isPrimitiveRoot_8563 : IsPrimitiveRoot zeta_int8563 8563 := by
  have h_prim : IsPrimitiveRoot (zeta8563 : K8563) 8563 := by
    convert IsCyclotomicExtension.zeta_spec 8563 ℚ K8563 using 1;
  have := h_prim.pow_eq_one;
  refine' ⟨ _, fun k hk => _ ⟩;
  · exact Subtype.ext this;
  · apply h_prim.2 k;
    convert congr_arg ( fun x : Ok8563 => x.val ) hk using 1

/-
State that Ok8563 is generated by zeta over Z.
-/
theorem Ok8563_eq_adjoin : (⊤ : Subalgebra ℤ Ok8563) = Algebra.adjoin ℤ {zeta_int8563} := by
  symm;
  -- Apply the theorem that states the adjoin of a primitive root of unity is the entire ring of integers.
  have h_adjoin : Algebra.adjoin ℤ {zeta_int8563} = ⊤ := by
    have h_prim_root : IsPrimitiveRoot zeta_int8563 8563 := by
      convert zeta_isPrimitiveRoot_8563 using 1
    have h_cyclotomic : IsCyclotomicExtension {8563} ℤ (NumberField.RingOfIntegers (CyclotomicField 8563 ℚ)) := by
      infer_instance;
    convert h_cyclotomic.adjoin_primitive_root_eq_top h_prim_root using 1;
  exact h_adjoin

/-
State the Artin property for any element x in Ok8563.
-/
theorem artin_property_8563 (N : ℕ) [Fact (Nat.Prime N)] (hN : N ≠ 8563) (P : Ideal Ok8563) [P.IsPrime] (hP : P.LiesOver (Ideal.span {(N : ℤ)})) (x : Ok8563) :
  Ideal.Quotient.mk P (sigma_int8563 (N_mod_8563 N hN) x) = (Ideal.Quotient.mk P x)^N := by
    have := @artin_property_zeta_8563;
    have h_gen : ∀ x ∈ Algebra.adjoin ℤ {zeta_int8563}, Ideal.Quotient.mk P (sigma_int8563 (N_mod_8563 N hN) x) = (Ideal.Quotient.mk P x)^N := by
      intro x hx;
      refine' Algebra.adjoin_induction _ _ _ _ hx;
      · aesop;
      · intro r; norm_cast;
        have := hP.1;
        rw [ eq_comm, Ideal.ext_iff ] at this;
        specialize this ( r ^ N - r ) ; simp_all +decide [ ← ZMod.intCast_zmod_eq_zero_iff_dvd, Ideal.mem_span_singleton ] ;
        exact Eq.symm ( Ideal.Quotient.eq.2 <| by simpa [ sub_eq_iff_eq_add ] using this );
      · simp +contextual [ ← map_add, ← map_pow ];
        intro x y hx hy hx' hy'; rw [ map_add ] ;
        rw [ add_pow ];
        rw [ Ideal.Quotient.eq ];
        -- Since $N$ is prime, the binomial coefficients $N.choose m$ for $1 \leq m \leq N-1$ are divisible by $N$.
        have h_binom : ∀ m ∈ Finset.Ico 1 N, (N.choose m : ℤ) ∈ Ideal.span {(N : ℤ)} := by
          exact fun m hm => Ideal.mem_span_singleton.mpr <| mod_cast Nat.dvd_of_mod_eq_zero <| by rw [ Nat.mod_eq_zero_of_dvd ] ; exact Nat.Prime.dvd_choose_self ( Fact.out : Nat.Prime N ) ( by linarith [ Finset.mem_Ico.mp hm ] ) ( by linarith [ Finset.mem_Ico.mp hm ] ) ;
        -- Since $P$ lies over the ideal spanned by $N$, any element in the ideal spanned by $N$ is in $P$.
        have h_ideal : ∀ m ∈ Finset.Ico 1 N, (N.choose m : Ok8563) ∈ P := by
          intros m hm;
          have h_ideal : Ideal.span {(N : ℤ)} ≤ P.comap (algebraMap ℤ Ok8563) := by
            exact hP.1.le;
          exact h_ideal ( h_binom m hm );
        rw [ Finset.sum_range_succ ];
        rw [ Finset.sum_eq_add_sum_diff_singleton ( Finset.mem_range.mpr ( Nat.Prime.pos Fact.out ) ) ];
        rw [ show ( Finset.range N \ { 0 } : Finset ℕ ) = Finset.Ico 1 N by ext ( _ | m ) <;> simp +decide ];
        rw [ show ( sigma_int8563 ( N_mod_8563 N hN ) ) x + ( sigma_int8563 ( N_mod_8563 N hN ) ) y - ( x ^ 0 * y ^ ( N - 0 ) * ↑ ( N.choose 0 ) + ∑ x_1 ∈ Finset.Ico 1 N, x ^ x_1 * y ^ ( N - x_1 ) * ↑ ( N.choose x_1 ) + x ^ N * y ^ ( N - N ) * ↑ ( N.choose N ) ) = ( sigma_int8563 ( N_mod_8563 N hN ) x - x ^ N ) + ( sigma_int8563 ( N_mod_8563 N hN ) y - y ^ N ) - ∑ x_1 ∈ Finset.Ico 1 N, x ^ x_1 * y ^ ( N - x_1 ) * ↑ ( N.choose x_1 ) by simp +decide [ sub_eq_iff_eq_add ] ; ring ];
        exact Ideal.sub_mem _ ( Ideal.add_mem _ ( Ideal.Quotient.eq.mp hx' ) ( Ideal.Quotient.eq.mp hy' ) ) ( Ideal.sum_mem _ fun i hi => Ideal.mul_mem_left _ _ ( h_ideal i hi ) );
      · simp +contextual [ *, mul_pow ];
    have h_gen : Algebra.adjoin ℤ {zeta_int8563} = ⊤ := by
      exact?;
    aesop

/-
State that sigma_n(eta_S) = eta_{nS}.
-/
lemma sigma_eta_8563 (n : (ZMod 8563)ˣ) (S : Finset (ZMod 8563)) :
  sigma_int8563 n (eta_int8563 S) = eta_int8563 (S.image (fun x => (n : ZMod 8563) * x)) := by
    unfold eta_int8563;
    rw [ Finset.sum_image ] <;> norm_num [ IsUnit.mul_right_injective ];
    refine' Finset.sum_congr rfl fun x hx => _;
    rw [ sigma_int_zeta_pow_8563 ];
    rw [ ← pow_mul, ZMod.val_mul ];
    rw [ ← Nat.mod_add_div ( ( n : ZMod 8563 ).val * x.val ) 8563, pow_add, pow_mul ] ; norm_num [ zeta_isPrimitiveRoot_8563.pow_eq_one ]

/-
State that multiplication by n in H fixes H, C1, and C2.
-/
lemma action_H_8563 (n : ZMod 8563) (hn : n ∈ H8563) :
  H8563.image (fun x => n * x) = H8563 ∧
  C1_8563.image (fun x => n * x) = C1_8563 ∧
  C2_8563.image (fun x => n * x) = C2_8563 := by
    simp [H8563, C1_8563, C2_8563] at hn ⊢;
    obtain ⟨ a, ha₀, rfl ⟩ := hn;
    constructor;
    · ext x;
      constructor;
      · simp +zetaDelta at *;
        exact fun y hy hx => ⟨ a * y, mul_ne_zero ha₀ hy, by linear_combination' hx ⟩;
      · simp +zetaDelta at *;
        intro y hy₀ hy₁; use y * a⁻¹; simp_all +decide [ mul_pow, mul_assoc, mul_comm, mul_left_comm ] ;
    · constructor <;> ext <;> simp +contextual [ Finset.mem_image, mul_assoc ];
      · constructor <;> rintro ⟨ b, hb₀, rfl ⟩;
        · exact ⟨ a * b, mul_ne_zero ha₀ hb₀, by ring ⟩;
        · -- Let $a_1 = b / a$.
          use b / a;
          grind;
      · constructor <;> rintro ⟨ b, hb, rfl ⟩;
        · exact ⟨ a * b, mul_ne_zero ha₀ hb, by ring ⟩;
        · -- Let $a_1 = b * a^{-1}$.
          use b * a⁻¹;
          grind

/-
State that multiplication by n in C1 permutes H->C1, C1->C2, C2->H.
-/
lemma action_C1_8563 (n : ZMod 8563) (hn : n ∈ C1_8563) :
  H8563.image (fun x => n * x) = C1_8563 ∧
  C1_8563.image (fun x => n * x) = C2_8563 ∧
  C2_8563.image (fun x => n * x) = H8563 := by
    have h_def : ∃ h ∈ H8563, n = g8563 * h := by
      unfold C1_8563 at hn; aesop;
    obtain ⟨ h, hh, rfl ⟩ := h_def;
    have h_perm : Finset.image (fun x => h * x) H8563 = H8563 ∧ Finset.image (fun x => h * x) C1_8563 = C1_8563 ∧ Finset.image (fun x => h * x) C2_8563 = C2_8563 := by
      exact?;
    have h_perm : Finset.image (fun x => g8563 * x) H8563 = C1_8563 ∧ Finset.image (fun x => g8563 * x) C1_8563 = C2_8563 ∧ Finset.image (fun x => g8563 * x) C2_8563 = H8563 := by
      native_decide +revert;
    have h_perm : Finset.image (fun x => g8563 * h * x) H8563 = Finset.image (fun x => g8563 * x) (Finset.image (fun x => h * x) H8563) ∧ Finset.image (fun x => g8563 * h * x) C1_8563 = Finset.image (fun x => g8563 * x) (Finset.image (fun x => h * x) C1_8563) ∧ Finset.image (fun x => g8563 * h * x) C2_8563 = Finset.image (fun x => g8563 * x) (Finset.image (fun x => h * x) C2_8563) := by
      simp +decide [ mul_assoc, Finset.ext_iff ];
    grind

/-
State that multiplication by n in C2 permutes H->C2, C1->H, C2->C1.
-/
lemma action_C2_8563 (n : ZMod 8563) (hn : n ∈ C2_8563) :
  H8563.image (fun x => n * x) = C2_8563 ∧
  C1_8563.image (fun x => n * x) = H8563 ∧
  C2_8563.image (fun x => n * x) = C1_8563 := by
    have h_image_H : Finset.image (fun x => n * x) H8563 = C2_8563 := by
      ext x; simp [C2_8563, H8563] at *; (
      obtain ⟨ a, ha, rfl ⟩ := hn; simp +decide [ mul_assoc, pow_succ, mul_comm, mul_left_comm ] ;
      constructor <;> intro h;
      · obtain ⟨ b, hb, rfl ⟩ := h; use a * b; simp +decide [ *, mul_assoc, mul_comm, mul_left_comm ] ;
      · obtain ⟨ b, hb, rfl ⟩ := h; use b * a⁻¹; simp +decide [ ha, hb, mul_assoc, mul_left_comm a ] ;);
    have h_image_C1 : Finset.image (fun x => n * x) C1_8563 = H8563 := by
      have h_image_C1 : Finset.image (fun x => n * x) C1_8563 = Finset.image (fun x => n * g8563 * x) H8563 := by
        ext; simp [C1_8563, H8563];
        simp +decide only [mul_assoc];
      have h_image_C1 : Finset.image (fun x => n * g8563 * x) H8563 = H8563 := by
        have h_n_g : n * g8563 ∈ H8563 := by
          obtain ⟨ x, hx, rfl ⟩ := Finset.mem_image.mp hn;
          obtain ⟨ y, hy, rfl ⟩ := Finset.mem_image.mp hx; ring_nf;
          exact Finset.mem_image.mpr ⟨ g8563 * y, Finset.mem_filter.mpr ⟨ Finset.mem_univ _, by
            exact mul_ne_zero ( by native_decide ) ( by simpa using hy ) ⟩, by ring ⟩
        have := action_H_8563 ( n * g8563 ) h_n_g; aesop;
      aesop
    have h_image_C2 : Finset.image (fun x => n * x) C2_8563 = C1_8563 := by
      have h_image_C2 : Finset.card (Finset.image (fun x => n * x) C2_8563) = Finset.card C1_8563 := by
        rw [ Finset.card_image_of_injective ];
        · native_decide +revert;
        · intro x y hxy;
          have h_inv : n ≠ 0 := by
            intro hn_zero
            simp [hn_zero] at hn;
            exact absurd hn ( by native_decide );
          haveI := Fact.mk ( by norm_num : Nat.Prime 8563 ) ; exact mul_left_cancel₀ h_inv hxy;
      refine' Finset.eq_of_subset_of_card_le ( _ ) ( by rw [ h_image_C2 ] );
      intro x hx
      obtain ⟨y, hy, rfl⟩ := Finset.mem_image.mp hx;
      obtain ⟨ z, hz, rfl ⟩ := Finset.mem_image.mp hy;
      obtain ⟨ w, hw, rfl ⟩ := Finset.mem_image.mp hn;
      obtain ⟨ u, hu, rfl ⟩ := Finset.mem_image.mp hz;
      -- Since $w \in H8563$, we can write $w = v^3$ for some $v \in \mathbb{Z}/8563\mathbb{Z}$.
      obtain ⟨ v, hv, rfl ⟩ : ∃ v : ZMod 8563, v ≠ 0 ∧ w = v^3 := by
        obtain ⟨ v, hv, rfl ⟩ := Finset.mem_image.mp hw;
        exact ⟨ v, by simpa using hv, rfl ⟩;
      exact Finset.mem_image.mpr ⟨ ( g8563 * u * v ) ^ 3, Finset.mem_image.mpr ⟨ g8563 * u * v, by
        simp +zetaDelta at *;
        exact ⟨ ⟨ by native_decide, hu ⟩, hv ⟩, rfl ⟩, by
        ring ⟩
    exact ⟨h_image_H, h_image_C1, h_image_C2⟩

/-
State that sigma_n permutes alpha to beta (if n in C1), gamma (if n in C2), or alpha (if n in H).
-/
lemma global_permutation_8563 (n : (ZMod 8563)ˣ) :
  ((n : ZMod 8563) ∈ C1_8563 → sigma_int8563 n alpha_int8563 = beta_int8563) ∧
  ((n : ZMod 8563) ∈ C2_8563 → sigma_int8563 n alpha_int8563 = gamma_int8563) ∧
  ((n : ZMod 8563) ∈ H8563 → sigma_int8563 n alpha_int8563 = alpha_int8563) := by
    unfold alpha_int8563 beta_int8563 gamma_int8563;
    unfold sign_8563;
    split_ifs <;> simp +decide [ *, map_mul ];
    · unfold alpha_prime_int8563 beta_prime_int8563 gamma_prime_int8563;
      unfold eta0_int8563 eta1_int8563 eta2_int8563; simp +decide [ map_sub, map_add, map_mul ] ;
      refine' ⟨ _, _, _ ⟩ <;> intro hn <;> rw [ sigma_eta_8563, sigma_eta_8563 ] <;> simp +decide [ hn, action_C1_8563, action_C2_8563, action_H_8563 ];
    · unfold alpha_prime_int8563 beta_prime_int8563 gamma_prime_int8563;
      unfold eta0_int8563 eta1_int8563 eta2_int8563; simp +decide [ map_sub, sigma_eta_8563 ] ;
      refine' ⟨ _, _, _ ⟩;
      · intro hn;
        rw [ action_C1_8563 _ hn |>.1, action_C1_8563 _ hn |>.2.1 ];
      · intro hn; rw [ action_C2_8563 _ hn |>.1, action_C2_8563 _ hn |>.2.1 ] ;
      · intro hn; rw [ action_H_8563 _ hn |>.1, action_H_8563 _ hn |>.2.1 ] ;

/-
State the Dream Theorem for q=8563.
-/
theorem final_result_8563 (N : ℕ) [Fact (Nat.Prime N)] (hN : N ≠ 8563) (hN_odd : N % 2 = 1) (hN_a : Nat.Coprime 185 N)
  (P : Ideal Ok8563) [P.IsMaximal] (hP : P.LiesOver (Ideal.span {(N : ℤ)}))
  (h_assum : DreamAssumptions_8563) :
  let alpha_bar := Ideal.Quotient.mk P alpha_int8563
  let beta_bar := Ideal.Quotient.mk P beta_int8563
  let gamma_bar := Ideal.Quotient.mk P gamma_int8563
  (chi8563 N = s1_8563 → alpha_bar ^ N = gamma_bar) ∧
  (chi8563 N = s2_8563 → alpha_bar ^ N = beta_bar) := by
    have := h_assum.2.2.1; ( have := h_assum.2.2.2; ( norm_num [ mul_assoc, ← pow_add ] at *; ) );
    have := h_assum.2.1; ( have := h_assum.1; ( norm_num [ mul_assoc, ← pow_add ] at *; ) );
    grind +ring

