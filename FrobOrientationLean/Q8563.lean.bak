def find_g_8563 : IO Unit := do
  let q := 8563
  let s2 := 5739
  let c (n : Nat) := (n : Nat) ^ 2854 % q
  let order := q - 1
  let factors := [2, 4281] -- 8562 = 2 * 4281. 4281 = 3 * 1427. 1427 is prime?
  -- 1427 is not divisible by 2,3,5. 1427 / 7 = 203.8. 1427 / 11 = 129. 1427 / 13 = 109.
  -- Actually I should just check primitivity properly or trust a small generator exists.
  -- 8562 = 2 * 3 * 1427.
  
  for g in [2:100] do
    let mut is_prim := true
    -- Check if g^((q-1)/p) != 1 for all prime factors p of q-1
    -- Factors of 8562: 2, 3, 1427
    if (g ^ (order / 2)) % q == 1 then is_prim := false
    if (g ^ (order / 3)) % q == 1 then is_prim := false
    if (g ^ (order / 1427)) % q == 1 then is_prim := false
    
    if is_prim then
      let val := c g
      if val == s2 then
        IO.println s!"Found generator g with c(g) = s2: {g}"
        return
      else
        IO.println s!"Found generator g with c(g) = {val} (not s2): {g}"

