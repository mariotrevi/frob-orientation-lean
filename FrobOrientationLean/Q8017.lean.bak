/-
Action of H on the cosets for q = 8017.
-/
lemma action_H_8017 (n : ZMod 8017) (hn : n ∈ H8017) :
  H8017.image (fun x => n * x) = H8017 ∧
  C1_8017.image (fun x => n * x) = C1_8017 ∧
