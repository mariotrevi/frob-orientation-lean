/-
The Dream Theorem for q = 7489.
-/
theorem final_result_7489 (N : ℕ) [Fact (Nat.Prime N)] (hN : N ≠ 7489) (hN_odd : N % 2 = 1) (hN_a : Nat.Coprime 173 N)
  (P : Ideal Ok7489) [P.IsMaximal] (hP : P.LiesOver (Ideal.span {(N : ℤ)}))
  (h_assum : DreamAssumptions_7489) :
  let alpha_bar := Ideal.Quotient.mk P alpha_int7489
  let beta_bar := Ideal.Quotient.mk P beta_int7489
  let gamma_bar := Ideal.Quotient.mk P gamma_int7489
  (chi7489 N = s1_7489 → alpha_bar ^ N = gamma_bar) ∧
  (chi7489 N = s2_7489 → alpha_bar ^ N = beta_bar) := by
    refine' ⟨ fun h => _, fun h => _ ⟩;
    · have := global_permutation_7489 ( N_mod_7489 N hN );
      have h_coset : (N_mod_7489 N hN : ZMod 7489) ∈ C2_7489 := by
        apply (mem_C2_iff_chi_eq_s1_7489 (N_mod_7489 N hN : ZMod 7489) (by
        simp_all +decide [ ZMod.natCast_eq_zero_iff ])).mpr h;
      rw [ ← this.2.1 h_coset, artin_property_7489 N hN P hP ];
    · have h_sigma_beta : Ideal.Quotient.mk P (sigma_int7489 (N_mod_7489 N hN) alpha_int7489) = Ideal.Quotient.mk P beta_int7489 := by
        have h_sigma_beta : (N_mod_7489 N hN : ZMod 7489) ∈ C1_7489 := by
          apply (mem_C1_iff_chi_eq_s2_7489 (N_mod_7489 N hN) (by
          simp +decide [ ZMod.natCast_eq_zero_iff ])).mpr h;
        exact congr_arg _ ( global_permutation_7489 _ |>.1 h_sigma_beta );
      rw [ ← h_sigma_beta, artin_property_7489 ];
      exact hP