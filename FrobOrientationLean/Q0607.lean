/-
The Frobenius automorphism permutes the Gaussian periods by shifting their index by ind(N).
-/
lemma sigma_period_int''' (N : ‚Ñï) [Fact (Nat.Prime N)] (hN : N ‚â† 607) (i : ZMod 3) :
  sigma_int607 (gal_unit607 N hN) (period_int''' i) = period_int''' (i + ind''' (N : ZMod 607)) := by
    -- Apply the lemma sigma_period''' to rewrite the goal in terms of the field periods.
    have h_field_period : sigma607 (gal_unit607 N hN) (period''' i) = period''' (i + ind''' (N : ZMod 607)) := by
      have h_field_period : ‚àÄ i : ZMod 3, sigma607 (gal_unit607 N hN) (period''' i) = period''' (i + ind''' (N : ZMod 607)) := by
        intro i
        have h_sigma_period : ‚àÄ x : ZMod 607, x ‚â† 0 ‚Üí sigma607 (gal_unit607 N hN) (Œ∂607 ^ (x.val : ‚Ñï)) = Œ∂607 ^ ((x * (N : ZMod 607)).val : ‚Ñï) := by
          intro x hx_nonzero
          have h_sigma_period : sigma607 (gal_unit607 N hN) (Œ∂607 ^ (x.val : ‚Ñï)) = (sigma607 (gal_unit607 N hN) Œ∂607) ^ (x.val : ‚Ñï) := by
            exact map_pow _ _ _;
          have h_sigma_period : sigma607 (gal_unit607 N hN) Œ∂607 = Œ∂607 ^ (N : ‚Ñï) := by
            have h_sigma_period : sigma607 (gal_unit607 N hN) Œ∂607 = (IsCyclotomicExtension.zeta_spec 607 ‚Ñö L607).toInteger ^ (N : ‚Ñï) := by
              convert sigma_zeta607 hN using 1;
              erw [ Subtype.mk_eq_mk ] ; aesop
            convert h_sigma_period using 1;
          simp_all +decide [ pow_mul', mul_comm ];
          have h_exp : (Œ∂607 ^ N) ^ x.val = Œ∂607 ^ (N * x.val) := by
            rw [ pow_mul ]
          have h_exp' : Œ∂607 ^ (N * x.val) = Œ∂607 ^ ((x * N).val : ‚Ñï) := by
            have h_exp' : Œ∂607 ^ (N * x.val) = Œ∂607 ^ ((N * x.val) % 607) := by
              have h_exp' : Œ∂607 ^ 607 = 1 := by
                exact IsPrimitiveRoot.pow_eq_one ( IsCyclotomicExtension.zeta_spec 607 ‚Ñö L607 );
              generalize_proofs at *; (
              rw [ ‚Üê Nat.mod_add_div ( N * x.val ) 607, pow_add, pow_mul ] ; aesop
              skip)
            have h_exp'' : (N * x.val) % 607 = (x * N).val := by
              norm_num [ mul_comm, ZMod.val_mul ]
            rw [h_exp', h_exp'']
          rw [h_exp, h_exp'];
        -- Apply the lemma sigma_period''' to rewrite the goal in terms of the field periods and then use the fact that the sum of the zeta powers is invariant under the permutation.
        have h_sum_inv : ‚àë x ‚àà C_coset''' i, Œ∂607 ^ ((x * (N : ZMod 607)).val : ‚Ñï) = ‚àë x ‚àà C_coset''' (i + ind''' (N : ZMod 607)), Œ∂607 ^ (x.val : ‚Ñï) := by
          apply Finset.sum_bij (fun x hx => x * (N : ZMod 607));
          ¬∑ intro x hx
            simp [C_coset''', ind'''] at hx ‚ä¢;
            -- Since $N$ is a prime not equal to $607$, it is non-zero in $ZMod 607$.
            have hN_nonzero : (N : ZMod 607) ‚â† 0 := by
              rw [ Ne.eq_def, ZMod.natCast_eq_zero_iff ] ; exact fun h => hN <| by have := Nat.prime_dvd_prime_iff_eq ( show Nat.Prime 607 by norm_num ) ( Fact.out : Nat.Prime N ) ; tauto;
            have h_mul_ind : ‚àÄ x y : ZMod 607, x ‚â† 0 ‚Üí y ‚â† 0 ‚Üí ind''' (x * y) = ind''' x + ind''' y := by
              native_decide +revert;
            have h_mul_ind : ind''' (x * (N : ZMod 607)) = ind''' x + ind''' (N : ZMod 607) := by
              exact h_mul_ind x _ hx.1 hN_nonzero;
            simp [ind'''] at h_mul_ind ‚ä¢;
            exact ‚ü® ‚ü® hx.1, hN_nonzero ‚ü©, by rw [ h_mul_ind, hx.2 ] ‚ü©;
          ¬∑ intro a‚ÇÅ ha‚ÇÅ a‚ÇÇ ha‚ÇÇ h_eq
            have h_inv : (N : ZMod 607) ‚â† 0 := by
              rw [ Ne.eq_def, ZMod.natCast_eq_zero_iff ] ; exact fun h => hN <| by have := Nat.prime_dvd_prime_iff_eq ( show Nat.Prime 607 by norm_num ) ( Fact.out : Nat.Prime N ) ; aesop;
            have h_inv_mul : a‚ÇÅ = a‚ÇÇ := by
              haveI := Fact.mk ( by norm_num : Nat.Prime 607 ) ; exact mul_right_cancel‚ÇÄ h_inv h_eq;
            exact h_inv_mul;
          ¬∑ intro b hb
            use b * (N : ZMod 607)‚Åª¬π;
            by_cases hN : ( N : ZMod 607 ) = 0 <;> simp_all +decide [ mul_assoc, mul_comm, mul_left_comm ];
            ¬∑ rw [ ZMod.natCast_eq_zero_iff ] at hN ; exact absurd ( Nat.dvd_of_mod_eq_zero ( show N % 607 = 0 from Nat.mod_eq_zero_of_dvd <| by exact hN ) ) ( by rw [ Nat.dvd_prime Fact.out ] ; aesop );
            ¬∑ have h_ind_mul : ind''' (b / (N : ZMod 607)) + ind''' (N : ZMod 607) = ind''' b := by
                have h_ind_mul : ind''' ((b / (N : ZMod 607)) * (N : ZMod 607)) = ind''' (b / (N : ZMod 607)) + ind''' (N : ZMod 607) := by
                  apply ind_mul''';
                  ¬∑ simp_all +decide [ C_coset''' ];
                  ¬∑ exact hN;
                rw [ ‚Üê h_ind_mul, div_mul_cancel‚ÇÄ _ hN ];
              unfold C_coset''' at *; aesop;
          ¬∑ exact fun _ _ => rfl;
        convert h_sum_inv using 1;
        ¬∑ rw [ period_eq_sum_C_coset''' ];
          rw [ map_sum, Finset.sum_congr rfl fun x hx => h_sigma_period x <| by
            exact Finset.mem_filter.mp hx |>.2.1 ];
        ¬∑ exact period_eq_sum_C_coset''' _;
      exact h_field_period i;
    have h_field_period : sigma_int607 (gal_unit607 N hN) (period_int''' i) = period_int''' (i + ind''' (N : ZMod 607)) := by
      have h_period_eq : (period_int''' i : L607) = period''' i := by
        -- By definition of `period_int'''`, we have `period_int''' i = ‚àë x ‚àà C_coset''' i, Œ∂607_int ^ (x.val : ‚Ñï)`.
        simp [period_int''', C_coset'''];
        rw [ period_eq_sum_C_coset''' ];
        unfold Œ∂607_int;
        unfold IsPrimitiveRoot.toInteger; aesop;
      have h_period_eq' : (period_int''' (i + ind''' (N : ZMod 607)) : L607) = period''' (i + ind''' (N : ZMod 607)) := by
        unfold period_int''';
        simp +decide [ period_eq_sum_C_coset''' ];
        unfold Œ∂607_int; aesop;
      convert h_field_period using 1;
      constructor <;> intro h <;> rw [ ‚Üê h_period_eq, ‚Üê h_period_eq' ] at * <;> norm_cast at *;
      ¬∑ convert congr_arg Subtype.val h using 1;
      ¬∑ exact Subtype.ext h;
    exact h_field_period

/-
If c''' N = s1''', then sigma_N(alpha''') = gamma'''.
-/
lemma sigma_alpha_eq_gamma_if_s1''' (N : ‚Ñï) [Fact (Nat.Prime N)] (hN : N ‚â† 607) (h : c''' N = s1''') :
  sigma_int607 (gal_unit607 N hN) Œ±'''_int = Œ≥'''_int := by
    -- By definition of $ind'''$, we know that $ind''' N = 1$ if and only if $c''' N = s1'''$.
    have h_ind_eq : ind''' (N : ZMod 607) = 1 := by
      exact?;
    -- By definition of $sigma_int607$, we know that $sigma_int607 (gal_unit607 N hN) (period_int''' i) = period_int''' (i + ind''' N)$.
    have h_sigma_period : ‚àÄ i : ZMod 3, sigma_int607 (gal_unit607 N hN) (period_int''' i) = period_int''' (i + ind''' (N : ZMod 607)) := by
      apply sigma_period_int''';
    -- Substitute the value of `ind''' N` into the hypothesis `h_sigma_period`.
    simp [h_ind_eq, Œ±'''_int, Œ≥'''_int] at *;
    convert congr_arg‚ÇÇ ( ¬∑ - ¬∑ ) ( h_sigma_period 2 ) ( h_sigma_period 1 ) using 1 ; ring;
    unfold sigma_int607; norm_num;
    exact?

/-
If c''' N = s2''', then sigma_N(alpha''') = beta'''.
-/
lemma sigma_alpha_eq_beta_if_s2''' (N : ‚Ñï) [Fact (Nat.Prime N)] (hN : N ‚â† 607) (h : c''' N = s2''') :
  sigma_int607 (gal_unit607 N hN) Œ±'''_int = Œ≤'''_int := by
    -- By definition of $c'''$, we know that $c''' N = s2'''$ implies $ind''' (N : ZMod 607) = 2$.
    have h_ind : ind''' (N : ZMod 607) = 2 := by
      exact?;
    unfold Œ±'''_int Œ≤'''_int;
    -- Apply the lemma that states sigma_int607 (gal_unit607 N hN) (period_int''' i) = period_int''' (i + ind''' (N : ZMod 607)).
    have h_sigma_period : sigma_int607 (gal_unit607 N hN) (period_int''' 1) = period_int''' (1 + ind''' (N : ZMod 607)) ‚àß sigma_int607 (gal_unit607 N hN) (period_int''' 2) = period_int''' (2 + ind''' (N : ZMod 607)) := by
      exact ‚ü® sigma_period_int''' N hN 1, sigma_period_int''' N hN 2 ‚ü©;
    convert congr_arg‚ÇÇ ( ¬∑ - ¬∑ ) h_sigma_period.2 h_sigma_period.1 using 1 ; ring;
    ¬∑ erw [ Subtype.mk_eq_mk ] ; norm_num ; ring!;
    ¬∑ rw [ show ( 2 + ind''' ( N : ZMod 607 ) ) = 1 by rw [ h_ind ] ; rfl, show ( 1 + ind''' ( N : ZMod 607 ) ) = 0 by rw [ h_ind ] ; rfl ] ; ring

/-
Final result for q=607: Frobenius action on alpha''' depends on c''' N.
-/
theorem final_result_607 {N : ‚Ñï} [Fact (Nat.Prime N)] (hN : N ‚â† 607) (P : Ideal ùìûL607) [P.IsMaximal] [CharP (kP607 P) N] :
  (c''' N = s1''' ‚Üí (red607 P Œ±'''_int) ^ N = red607 P Œ≥'''_int) ‚àß
  (c''' N = s2''' ‚Üí (red607 P Œ±'''_int) ^ N = red607 P Œ≤'''_int) := by
  constructor
  ¬∑ intro h
    rw [‚Üê artin_property607 hN P Œ±'''_int]
    rw [sigma_alpha_eq_gamma_if_s1''' N hN h]
  ¬∑ intro h
    rw [‚Üê artin_property607 hN P Œ±'''_int]
    rw [sigma_alpha_eq_beta_if_s2''' N hN h]
