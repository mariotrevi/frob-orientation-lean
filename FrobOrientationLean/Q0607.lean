/-
sigma_int607 maps zeta607_int to zeta607_int^N.
-/
lemma sigma_zeta607 {N : ‚Ñï} [Fact (Nat.Prime N)] (hN : N ‚â† 607) :
  sigma_int607 (gal_unit607 N hN) Œ∂607_int = Œ∂607_int ^ N := by
    have h_sigma_zeta : sigma607 (gal_unit607 N hN) (IsCyclotomicExtension.zeta 607 ‚Ñö L607) = IsCyclotomicExtension.zeta 607 ‚Ñö L607 ^ N := by
      have h_sigma_zeta : ‚àÄ (u : (ZMod 607)À£), sigma607 u (IsCyclotomicExtension.zeta 607 ‚Ñö L607) = IsCyclotomicExtension.zeta 607 ‚Ñö L607 ^ (u.val.val : ‚Ñï) := by
        intro u
        unfold sigma607
        simp [IsCyclotomicExtension.autEquivPow];
        erw [ PowerBasis.equivOfMinpoly_gen ];
        simp +decide [ IsPrimitiveRoot.powerBasis ];
      convert h_sigma_zeta ( gal_unit607 N hN ) using 1;
      have h_order : IsCyclotomicExtension.zeta 607 ‚Ñö L607 ^ 607 = 1 := by
        exact IsPrimitiveRoot.pow_eq_one ( IsCyclotomicExtension.zeta_spec 607 ‚Ñö L607 );
      have h_order : IsCyclotomicExtension.zeta 607 ‚Ñö L607 ^ (N % 607) = IsCyclotomicExtension.zeta 607 ‚Ñö L607 ^ N := by
        rw [ ‚Üê Nat.mod_add_div N 607, pow_add, pow_mul ] ; aesop;
      convert h_order.symm using 1;
    unfold sigma_int607 Œ∂607_int; aesop;

/-
The automorphism sigma_N reduces to the Frobenius map modulo P.
-/
theorem artin_property607 {N : ‚Ñï} [Fact (Nat.Prime N)] (hN : N ‚â† 607) (P : Ideal ùìûL607) [P.IsMaximal] [CharP (kP607 P) N] (x : ùìûL607) :
  red607 P (sigma_int607 (gal_unit607 N hN) x) = (red607 P x) ^ N := by
    have h_frobenius : ‚àÄ (x : ùìûL607), red607 P (sigma_int607 (gal_unit607 N hN) x) = (red607 P x) ^ N := by
      have := adjoin_zeta_eq_top607
      intro x
      have hx : x ‚àà Algebra.adjoin ‚Ñ§ ({Œ∂607_int} : Set (ùìû L607)) := by
        aesop
        skip
      generalize_proofs at *; (
      refine' Algebra.adjoin_induction _ _ _ _ hx <;> norm_num +zetaDelta at *;
      ¬∑ have h_frobenius : sigma_int607 (gal_unit607 N hN) Œ∂607_int = Œ∂607_int ^ N := by
          exact?
        generalize_proofs at *; (
        rw [ h_frobenius, map_pow ]);
      ¬∑ intro r
        simp [sigma_int607];
        -- Since $r$ is an integer, we have $r^N \equiv r \pmod{N}$ by Fermat's little theorem.
        have h_fermat : (r : kP607 P) ^ N = r := by
          have h_fermat : ‚àÄ (r : ‚Ñ§), (r : kP607 P) ^ N = r := by
            intro r; induction r <;> simp_all +decide [ pow_succ' ] ;
            ¬∑ exact Nat.Prime.ne_zero Fact.out;
            ¬∑ haveI := Fact.mk ( Fact.out : Nat.Prime N ) ; simp_all +decide [ add_pow_char ] ;
            ¬∑ haveI := Fact.mk ( Fact.out : Nat.Prime N ) ; simp_all +decide [ sub_eq_add_neg, add_pow_char ] ;
              exact?
          generalize_proofs at *; (
          grind)
        generalize_proofs at *; (
        exact h_fermat.symm ‚ñ∏ rfl
        skip);
      ¬∑ intro x y hx hy hx' hy'; simp_all +decide [ map_add, add_pow_char ] ;
        convert congr_arg‚ÇÇ ( ¬∑ + ¬∑ ) hx' hy' using 1 ; ring!;
        unfold sigma_int607; aesop;
      ¬∑ intro x y hx hy hx' hy'; simp +decide [ *, mul_pow ] ;
        convert congr_arg‚ÇÇ ( ¬∑ * ¬∑ ) hx' hy' using 1 ; ring!;
        unfold sigma_int607; aesop;)
    generalize_proofs at *; (
    exact h_frobenius x)

/-
