lemma ind_N_eq_1_iff_c_eq_s1''' (N : ‚Ñï) [Fact (Nat.Prime N)] (hN : N ‚â† 607) :
  ind''' (N : ZMod 607) = 1 ‚Üî c''' N = s1''' := by
    apply Iff.intro (fun h => by
      have := ind_spec''' N (by
      exact?);
