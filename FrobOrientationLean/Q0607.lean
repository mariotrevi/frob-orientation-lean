/-
Prove that the sum of the periods is -1.
-/
lemma sum_eta''' : Œ∑‚ÇÄ''' + Œ∑‚ÇÅ''' + Œ∑‚ÇÇ''' = -1 := by
  -- By definition of $S_cubic_res'''$, $S_s1'''$, and $S_s2'''$, we know that every non-zero element of $\mathbb{F}_{607}$ is in exactly one of these sets.
  have h_union : S_cubic_res''' ‚à™ S_s1''' ‚à™ S_s2''' = Finset.univ.erase 0 := by
    native_decide +revert;
  -- By definition of $Œ∑‚ÇÄ'''$, $Œ∑‚ÇÅ'''$, and $Œ∑‚ÇÇ'''$, we can write
  have h_sum : Œ∑‚ÇÄ''' + Œ∑‚ÇÅ''' + Œ∑‚ÇÇ''' = ‚àë x ‚àà S_cubic_res''' ‚à™ S_s1''' ‚à™ S_s2''', Œ∂607 ^ (x.val : ‚Ñï) := by
    rw [ Finset.sum_union, Finset.sum_union ] <;> norm_num [ Finset.disjoint_left ];
    ¬∑ exact?;
    ¬∑ native_decide +revert;
    ¬∑ native_decide +revert;
  -- The sum of all powers of Œ∂607 from 0 to 606 is zero because Œ∂607 is a primitive 607th root of unity.
  have h_sum_zero : ‚àë x ‚àà Finset.range 607, Œ∂607 ^ x = 0 := by
    rw [ geom_sum_eq ] <;> norm_num [ IsPrimitiveRoot.ne_one ];
    ¬∑ exact Or.inl ( sub_eq_zero_of_eq <| by exact ( IsCyclotomicExtension.zeta_spec 607 ‚Ñö L607 ) |> fun h => h.pow_eq_one );
    ¬∑ exact IsPrimitiveRoot.ne_one ( IsCyclotomicExtension.zeta_spec 607 ‚Ñö L607 ) ( by decide );
  -- The sum over the union of the sets is the same as the sum over all elements except 0.
  have h_sum_union : ‚àë x ‚àà S_cubic_res''' ‚à™ S_s1''' ‚à™ S_s2''', Œ∂607 ^ (x.val : ‚Ñï) = ‚àë x ‚àà Finset.range 607, Œ∂607 ^ x - 1 := by
    rw [ h_union ];
    rw [ Finset.sum_erase_eq_sub ( Finset.mem_univ 0 ) ] ; norm_num;
    rfl;
  rw [ h_sum, h_sum_union, h_sum_zero, zero_sub ]

/-
The number of solutions to x+y=u depends only on the index of u. Specifically, N_sol(i, j, u) = cyc_num(i - ind(u), j - ind(u)).
-/
lemma N_sol_eq_cyc_num''' (i j : ZMod 3) (u : ZMod 607) (hu : u ‚â† 0) :
  N_sol''' i j u = cyc_num''' (i - ind''' u) (j - ind''' u) := by
    apply Fintype.card_congr;
    refine' Equiv.ofBijective ( fun p => ‚ü® p.val.1 / u, _, _, _, _ ‚ü© ) ‚ü® _, _ ‚ü©;
    all_goals norm_num [ Function.Injective, Function.Surjective ];
    grind;
    ¬∑ grind;
    ¬∑ have h_ind_div : ind''' ((p.val.1 / u)) + ind''' u = ind''' p.val.1 := by
        have h_ind_div : ind''' ((p.val.1 / u) * u) = ind''' (p.val.1 / u) + ind''' u := by
          apply ind_mul''';
          ¬∑ grind;
          ¬∑ exact hu;
        rw [ ‚Üê h_ind_div, div_mul_cancel‚ÇÄ _ hu ];
      grind;
    ¬∑ have h_ind_mul : ind''' (u‚Åª¬π * p.val.2) = ind''' (u‚Åª¬π) + ind''' p.val.2 := by
        apply ind_mul''';
        ¬∑ haveI := Fact.mk ( by norm_num : Nat.Prime 607 ) ; exact inv_ne_zero hu;
        ¬∑ exact p.2.2.1;
      have h_ind_inv : ind''' u‚Åª¬π = -ind''' u := by
        exact ind_inv''' u hu;
      grind;
    ¬∑ intros; rw [ div_eq_div_iff ] at * <;> aesop;
    ¬∑ intro a ha‚ÇÅ ha‚ÇÇ ha‚ÇÉ ha‚ÇÑ
      use a * u;
      have h_ind_mul : ind''' (a * u) = ind''' a + ind''' u := by
        exact ind_mul''' a u ha‚ÇÅ hu;
      have h_ind_mul : ind''' (u * (1 - a)) = ind''' u + ind''' (1 - a) := by
        apply ind_mul''';
        ¬∑ assumption;
        ¬∑ exact sub_ne_zero_of_ne <| Ne.symm ha‚ÇÇ;
      grind +ring

/-
Multiplication rule for Gaussian periods for q=607.
-/
lemma period_mul''' (i j : ZMod 3) :
  period''' i * period''' j = (if i = j then 202 else 0) +
  (cyc_num''' (i - 0) (j - 0) * period''' 0 +
   cyc_num''' (i - 1) (j - 1) * period''' 1 +
   cyc_num''' (i - 2) (j - 2) * period''' 2) := by
     have h_expand : period''' i * period''' j = (if i = j then 202 else 0) + ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0), (N_sol''' i j u : L607) * Œ∂607 ^ (u.val : ‚Ñï) := by
       -- By definition of $N_sol'''$, we can rewrite the sum as $\sum_{u \neq 0} N_sol''' i j u \cdot \zeta^{u.val}$.
       have h_sum : ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0), (N_sol''' i j u : L607) * Œ∂607 ^ (u.val : ‚Ñï) = ‚àë x ‚àà C_coset''' i, ‚àë y ‚àà C_coset''' j, Œ∂607 ^ ((x + y).val : ‚Ñï) - (if i = j then 202 else 0) := by
         have h_sum : ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0), (N_sol''' i j u : L607) * Œ∂607 ^ (u.val : ‚Ñï) = ‚àë x ‚àà C_coset''' i, ‚àë y ‚àà C_coset''' j, Œ∂607 ^ ((x + y).val : ‚Ñï) - ‚àë x ‚àà C_coset''' i, ‚àë y ‚àà C_coset''' j, if x + y = 0 then Œ∂607 ^ ((x + y).val : ‚Ñï) else 0 := by
           have h_sum : ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0), (N_sol''' i j u : L607) * Œ∂607 ^ (u.val : ‚Ñï) = ‚àë x ‚àà C_coset''' i, ‚àë y ‚àà C_coset''' j, Œ∂607 ^ ((x + y).val : ‚Ñï) - ‚àë x ‚àà C_coset''' i, ‚àë y ‚àà C_coset''' j, if x + y = 0 then Œ∂607 ^ ((x + y).val : ‚Ñï) else 0 := by
             have h_sum_eq : ‚àÄ u : ZMod 607, u ‚â† 0 ‚Üí (N_sol''' i j u : L607) = ‚àë x ‚àà C_coset''' i, ‚àë y ‚àà C_coset''' j, if x + y = u then 1 else 0 := by
               intros u hu
               have h_card : Fintype.card { p : ZMod 607 √ó ZMod 607 // p.1 ‚â† 0 ‚àß p.2 ‚â† 0 ‚àß ind''' p.1 = i ‚àß ind''' p.2 = j ‚àß p.1 + p.2 = u } = ‚àë x ‚àà C_coset''' i, ‚àë y ‚àà C_coset''' j, if x + y = u then 1 else 0 := by
                 rw [ Fintype.card_subtype ];
                 rw [ show ( Finset.filter ( fun x : ZMod 607 √ó ZMod 607 => x.1 ‚â† 0 ‚àß x.2 ‚â† 0 ‚àß ind''' x.1 = i ‚àß ind''' x.2 = j ‚àß x.1 + x.2 = u ) Finset.univ ) = Finset.filter ( fun x : ZMod 607 √ó ZMod 607 => x.1 + x.2 = u ) ( Finset.product ( C_coset''' i ) ( C_coset''' j ) ) from ?_ ];
                 ¬∑ erw [ Finset.card_filter ];
                   erw [ Finset.sum_product ];
                 ¬∑ ext ‚ü®x, y‚ü©; simp [C_coset'''];
                   tauto;
               exact_mod_cast h_card
             have h_sum_eq : ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0), (N_sol''' i j u : L607) * Œ∂607 ^ (u.val : ‚Ñï) = ‚àë x ‚àà C_coset''' i, ‚àë y ‚àà C_coset''' j, ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0), if x + y = u then Œ∂607 ^ (u.val : ‚Ñï) else 0 := by
               rw [ Finset.sum_congr rfl fun u hu => by rw [ h_sum_eq u ( Finset.mem_filter.mp hu |>.2 ) ] ];
               simp +decide only [Finset.sum_mul _ _ _];
               exact Finset.sum_comm.trans ( Finset.sum_congr rfl fun _ _ => Finset.sum_comm.trans ( Finset.sum_congr rfl fun _ _ => Finset.sum_congr rfl fun _ _ => by split_ifs <;> ring ) );
             rw [h_sum_eq];
             simp +decide [ Finset.sum_ite, Finset.filter_ne' ];
             simp +decide only [Finset.sum_filter, ‚Üê Finset.sum_sub_distrib];
             exact Finset.sum_congr rfl fun x hx => Finset.sum_congr rfl fun y hy => by split_ifs <;> simp +decide [ * ] ;
           convert h_sum using 1;
         -- The sum of the if statements is the number of pairs (x, y) in C_coset''' i √ó C_coset''' j such that x + y = 0.
         have h_if_sum : ‚àë x ‚àà C_coset''' i, ‚àë y ‚àà C_coset''' j, (if x + y = 0 then 1 else 0) = if i = j then 202 else 0 := by
           fin_cases i <;> fin_cases j <;> native_decide;
         rw [ h_sum, ‚Üê Nat.cast_inj ( R := L607 ) ] at * ; aesop;
       rw [ period_eq_sum_C_coset''', h_sum ];
       rw [ show period''' j = ‚àë y ‚àà C_coset''' j, Œ∂607 ^ ( y.val : ‚Ñï ) by exact? ] ; simp +decide [ Finset.sum_mul _ _ _ ] ; ring;
       refine' Finset.sum_congr rfl fun x hx => _;
       rw [ Finset.mul_sum _ _ _ ] ; refine' Finset.sum_congr rfl fun y hy => _ ; rw [ ‚Üê pow_add ] ; rw [ ZMod.val_add ] ; ring;
       rw [ ‚Üê pow_add, ‚Üê Nat.mod_add_div ( x.val + y.val ) 607 ] ; norm_num [ pow_add, pow_mul, IsPrimitiveRoot.pow_eq_one ] ;
       have hŒ∂607 : Œ∂607 ^ 607 = 1 := by
         exact IsPrimitiveRoot.pow_eq_one ( IsCyclotomicExtension.zeta_spec 607 ‚Ñö L607 );
       rw [ hŒ∂607, one_pow, mul_one ];
     -- Apply the N_sol_eq_cyc_num''' lemma to rewrite the sum.
     have h_rewrite : ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0), (N_sol''' i j u : L607) * Œ∂607 ^ (u.val : ‚Ñï) = ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0), (cyc_num''' (i - ind''' u) (j - ind''' u) : L607) * Œ∂607 ^ (u.val : ‚Ñï) := by
       exact Finset.sum_congr rfl fun x hx => by rw [ N_sol_eq_cyc_num''' i j x ( by simpa using hx ) ] ;
     -- Split the sum into three parts based on the value of `ind''' u`.
     have h_split : ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0), (cyc_num''' (i - ind''' u) (j - ind''' u) : L607) * Œ∂607 ^ (u.val : ‚Ñï) = ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0 ‚àß ind''' x = 0), (cyc_num''' (i - 0) (j - 0) : L607) * Œ∂607 ^ (u.val : ‚Ñï) + ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0 ‚àß ind''' x = 1), (cyc_num''' (i - 1) (j - 1) : L607) * Œ∂607 ^ (u.val : ‚Ñï) + ‚àë u ‚àà Finset.univ.filter (fun x : ZMod 607 => x ‚â† 0 ‚àß ind''' x = 2), (cyc_num''' (i - 2) (j - 2) : L607) * Œ∂607 ^ (u.val : ‚Ñï) := by
       have h_split : ‚àÄ u : ZMod 607, u ‚â† 0 ‚Üí (cyc_num''' (i - ind''' u) (j - ind''' u) : L607) * Œ∂607 ^ (u.val : ‚Ñï) = (if ind''' u = 0 then (cyc_num''' (i - 0) (j - 0) : L607) * Œ∂607 ^ (u.val : ‚Ñï) else 0) + (if ind''' u = 1 then (cyc_num''' (i - 1) (j - 1) : L607) * Œ∂607 ^ (u.val : ‚Ñï) else 0) + (if ind''' u = 2 then (cyc_num''' (i - 2) (j - 2) : L607) * Œ∂607 ^ (u.val : ‚Ñï) else 0) := by
         intro u hu
         have h_ind : ind''' u = 0 ‚à® ind''' u = 1 ‚à® ind''' u = 2 := by
           native_decide +revert;
         grind;
       rw [ Finset.sum_congr rfl fun x hx => h_split x <| Finset.mem_filter.mp hx |>.2 ];
       simp +decide only [Finset.sum_add_distrib, Finset.sum_filter];
       simp +contextual only [ite_and];
     rw [ h_expand, h_rewrite, h_split ];
     rw [ period_eq_sum_C_coset''', period_eq_sum_C_coset''', period_eq_sum_C_coset''' ];
     simp +decide only [C_coset''', Finset.mul_sum _ _ _]

