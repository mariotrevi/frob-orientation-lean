        intro u
        have h_group : ‚àë x ‚àà C_coset i, ‚àë y ‚àà C_coset j, (if x + y = u then 1 else 0) = (if u = 0 then (if i = j then 32 else 0) else (N_sol i j u : ‚Ñï)) := by
          split_ifs <;> simp_all +decide [ N_sol ];
          ¬∑ fin_cases j <;> native_decide;
