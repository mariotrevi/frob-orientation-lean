lemma sigma_eta_3313 (n : (ZMod 3313)ˣ) (S : Finset (ZMod 3313)) :
  sigma_int3313 n (eta_int3313 S) = eta_int3313 (S.image (fun x => (n : ZMod 3313) * x)) := by
