theorem DreamTheorem_3313 (N : ℕ) [Fact (Nat.Prime N)] (hN : N ≠ 3313) (hN_odd : N % 2 = 1) (hN_a : Nat.Coprime 115 N)
  (P : Ideal Ok3313) [P.IsMaximal] (hP : P.LiesOver (Ideal.span {(N : ℤ)}))
  (h_assum : DreamAssumptions_3313) :
