abbrev K5557 := CyclotomicField 5557 ℚ

noncomputable def zeta5557 : K5557 := IsCyclotomicExtension.zeta 5557 ℚ K5557

/-
Define subgroups and periods.
-/
def g5557 : ZMod 5557 := 5

def H5557 : Finset (ZMod 5557) := ((Finset.univ : Finset (ZMod 5557)).filter (fun x => x ≠ 0)).image (fun x => x^3)

def C1_5557 : Finset (ZMod 5557) := H5557.image (fun x => g5557 * x)

def C2_5557 : Finset (ZMod 5557) := H5557.image (fun x => g5557^2 * x)

open scoped BigOperators

noncomputable def eta5557 (S : Finset (ZMod 5557)) : K5557 := ∑ x ∈ S, zeta5557^(x.val)

noncomputable def eta0_5557 := eta5557 H5557
noncomputable def eta1_5557 := eta5557 C1_5557
noncomputable def eta2_5557 := eta5557 C2_5557

noncomputable def alpha_prime_5557 := eta0_5557 - eta1_5557
noncomputable def beta_prime_5557 := eta1_5557 - eta2_5557
noncomputable def gamma_prime_5557 := eta2_5557 - eta0_5557

/-
Define the ring of integers and state irreducibility of the cyclotomic polynomial.
-/
abbrev Ok5557 := NumberField.RingOfIntegers K5557

theorem h_irr_5557 : Irreducible (Polynomial.cyclotomic 5557 ℚ) := by
  exact Polynomial.cyclotomic.irreducible_rat ( by norm_num )

/-
Define automorphisms and state integrality of zeta.
-/
noncomputable def sigma5557 (n : (ZMod 5557)ˣ) : K5557 ≃ₐ[ℚ] K5557 :=
  (IsCyclotomicExtension.autEquivPow K5557 h_irr_5557).symm n

theorem zeta_isIntegral_5557 : IsIntegral ℤ zeta5557 := by
  refine' ⟨ Polynomial.X ^ 5557 - 1, _, _ ⟩;
  · exact Polynomial.monic_X_pow_sub_C _ ( by norm_num );
  · have h_zeta_root : IsPrimitiveRoot zeta5557 5557 := by
      convert IsCyclotomicExtension.zeta_spec 5557 ℚ K5557 using 1;
    simp +decide [ h_zeta_root.pow_eq_one ]

/-
Define integral versions of zeta, sigma, and periods.
-/
def zeta_int5557 : Ok5557 := ⟨zeta5557, zeta_isIntegral_5557⟩

noncomputable def sigma_int5557 (n : (ZMod 5557)ˣ) : Ok5557 ≃ₐ[ℤ] Ok5557 :=
  (NumberField.RingOfIntegers.mapAlgEquiv (sigma5557 n)).restrictScalars ℤ

noncomputable def eta_int5557 (S : Finset (ZMod 5557)) : Ok5557 := ∑ x ∈ S, zeta_int5557^(x.val)

noncomputable def eta0_int5557 := eta_int5557 H5557
noncomputable def eta1_int5557 := eta_int5557 C1_5557
noncomputable def eta2_int5557 := eta_int5557 C2_5557

noncomputable def alpha_prime_int5557 := eta0_int5557 - eta1_int5557
noncomputable def beta_prime_int5557 := eta1_int5557 - eta2_int5557
noncomputable def gamma_prime_int5557 := eta2_int5557 - eta0_int5557

/-
Define signed periods.
-/
open Classical

noncomputable def sign_5557 : ℤ :=
  if alpha_prime_int5557 * beta_prime_int5557 * gamma_prime_int5557 = (5557 : Ok5557) then 1 else -1

noncomputable def alpha_int5557 := (sign_5557 : Ok5557) * alpha_prime_int5557
noncomputable def beta_int5557 := (sign_5557 : Ok5557) * beta_prime_int5557
noncomputable def gamma_int5557 := (sign_5557 : Ok5557) * gamma_prime_int5557

noncomputable def alpha5557_field : K5557 := alpha_int5557
noncomputable def beta5557_field : K5557 := beta_int5557
noncomputable def gamma5557_field : K5557 := gamma_int5557

/-
Define the assumptions for the Dream Theorem.
-/
def DreamAssumptions_5557 : Prop :=
  alpha_int5557^3 = 5557 * alpha_int5557 + 5557 ∧
  alpha_int5557 * beta_int5557 * gamma_int5557 = 5557 ∧
  149 * beta_int5557 = 3 * alpha_int5557^2 - 79 * alpha_int5557 - 11114 ∧
  149 * gamma_int5557 = -3 * alpha_int5557^2 + 70 * alpha_int5557 + 11114

/-
Define N mod 5557 as a unit and state the action of sigma on zeta.
-/
def N_mod_5557 (N : ℕ) [Fact (Nat.Prime N)] (hN : N ≠ 5557) : (ZMod 5557)ˣ :=
  ZMod.unitOfCoprime N (Nat.Coprime.symm ((Nat.coprime_primes (by norm_num) (Fact.out)).mpr hN.symm))

lemma sigma_int_zeta_pow_5557 (n : (ZMod 5557)ˣ) :
  sigma_int5557 n zeta_int5557 = zeta_int5557 ^ (n : ZMod 5557).val := by
    -- By definition of $sigma$, we know that $sigma_int5557 n = (sigma5557 n).restrictScalars ℤ$.
    simp [sigma_int5557, sigma5557];
    unfold zeta_int5557;
    erw [ Subtype.mk_eq_mk ];
    erw [ PowerBasis.equivOfMinpoly_gen ];
    aesop

/-
State the Artin property for zeta.
-/
theorem artin_property_zeta_5557 (N : ℕ) [Fact (Nat.Prime N)] (hN : N ≠ 5557) (P : Ideal Ok5557) [P.IsPrime] (hP : P.LiesOver (Ideal.span {(N : ℤ)})) :
  Ideal.Quotient.mk P (sigma_int5557 (N_mod_5557 N hN) zeta_int5557) = (Ideal.Quotient.mk P zeta_int5557)^N := by
    have h_sigma_zeta : Ideal.Quotient.mk P (sigma_int5557 (N_mod_5557 N hN) zeta_int5557) = (Ideal.Quotient.mk P zeta_int5557) ^ N := by
      have h_sigma_zeta_def : sigma_int5557 (N_mod_5557 N hN) zeta_int5557 = zeta_int5557 ^ (N : ZMod 5557).val := by
        convert sigma_int_zeta_pow_5557 ( N_mod_5557 N hN ) using 1
      rw [ h_sigma_zeta_def, ← Nat.mod_add_div N 5557, pow_add, pow_mul ] ; norm_num [ pow_add, pow_mul, Nat.mul_mod, Nat.pow_mod ] ; ring;
      erw [ show ( N + N / 5557 * 5557 : ZMod 5557 ).val = N % 5557 from ?_ ];
      · have h_sigma_zeta_def : (Ideal.Quotient.mk P zeta_int5557) ^ 5557 = 1 := by
          have h_sigma_zeta_def : zeta_int5557 ^ 5557 = 1 := by
            have h_cong : zeta5557 ^ 5557 = 1 := by
              exact IsCyclotomicExtension.zeta_pow _ _ _;
            exact Subtype.ext h_cong;
          rw [ ← map_pow, h_sigma_zeta_def, map_one ];
        rw [ pow_mul', h_sigma_zeta_def, one_pow, mul_one ];
      · norm_num [ ZMod.val_add, ZMod.val_mul ];
        exact?;
    exact h_sigma_zeta

/-
Prove that zeta is a primitive root of unity.
-/
theorem zeta_isPrimitiveRoot_5557 : IsPrimitiveRoot zeta5557 5557 := by
  -- By definition of $zeta5557$, we know that it is a primitive root of unity of order 5557. This follows from the properties of cyclotomic fields.
  apply IsCyclotomicExtension.zeta_spec 5557 ℚ K5557

/-
Prove that the integral zeta is a primitive root of unity.
-/
theorem zeta_int_isPrimitiveRoot_5557 : IsPrimitiveRoot zeta_int5557 5557 := by
  have h_prim : IsPrimitiveRoot (zeta5557) 5557 := by
    exact?;
  constructor;
  · exact Subtype.ext <| h_prim.pow_eq_one;
  · intro l hl
    have h_eq : (zeta5557 : K5557) ^ l = 1 := by
      convert congr_arg Subtype.val hl using 1;
    exact h_prim.2 l h_eq

/-
Verify that the ring of integers is a cyclotomic extension.
-/
theorem Ok5557_is_cyclotomic : IsCyclotomicExtension {5557} ℤ Ok5557 := by
  -- The cyclotomic field is generated by a primitive 5557-th root of unity, which is an algebraic integer.
  have h_gen : IsCyclotomicExtension {5557} ℚ K5557 := by
    infer_instance;
  have := h_gen;
  exact?

/-
Prove that the ring of integers is generated by zeta.
-/
theorem Ok5557_eq_adjoin : (⊤ : Subalgebra ℤ Ok5557) = Algebra.adjoin ℤ {zeta_int5557} := by
  rw [ eq_comm ];
  convert Ok5557_is_cyclotomic.adjoin_primitive_root_eq_top _;
  convert zeta_int_isPrimitiveRoot_5557 using 1

/-
Prove the Artin property for general elements.
-/
theorem artin_property_5557 (N : ℕ) [Fact (Nat.Prime N)] (hN : N ≠ 5557) (P : Ideal Ok5557) [P.IsPrime] (hP : P.LiesOver (Ideal.span {(N : ℤ)})) (x : Ok5557) :
  Ideal.Quotient.mk P (sigma_int5557 (N_mod_5557 N hN) x) = (Ideal.Quotient.mk P x)^N := by
    have h_adjoin : ∀ x ∈ Algebra.adjoin ℤ {zeta_int5557}, (Ideal.Quotient.mk P) ((sigma_int5557 (N_mod_5557 N hN)) x) = (Ideal.Quotient.mk P x) ^ N := by
      intro x hx
      induction' hx using Algebra.adjoin_induction with x hx ihx;
      · have := artin_property_zeta_5557 N hN P hP; aesop;
      · erw [ Ideal.Quotient.eq ];
        have h_fermat : (algebraMap ℤ Ok5557) ihx ^ N - (algebraMap ℤ Ok5557) ihx ∈ P := by
          have h_fermat : (algebraMap ℤ Ok5557) (ihx ^ N - ihx) ∈ P := by
            have h_fermat : (ihx ^ N - ihx : ℤ) ∈ Ideal.span {(N : ℤ)} := by
              exact Ideal.mem_span_singleton.mpr ( by haveI := Fact.mk ( Fact.out : Nat.Prime N ) ; simp +decide [ ← ZMod.intCast_zmod_eq_zero_iff_dvd ] )
            have := hP.1;
            exact this ▸ h_fermat |> fun h => by simpa using h;
          aesop;
        convert neg_mem h_fermat using 1 ; ring!;
        erw [ map_intCast ] ; aesop;
      · rename_i x y hx hy ihx ihy;
        have h_frobenius : ∀ (a b : Ok5557 ⧸ P), (a + b) ^ N = a ^ N + b ^ N := by
          have h_frobenius : ∀ (a b : Ok5557 ⧸ P), (a + b) ^ N = a ^ N + b ^ N := by
            intro a b
            have h_char : ringChar (Ok5557 ⧸ P) = N := by
              have h_char : ringChar (Ok5557 ⧸ P) = ringChar (ℤ ⧸ Ideal.span {(N : ℤ)}) := by
                have h_char : Ideal.Quotient.mk P (algebraMap ℤ Ok5557 N) = 0 := by
                  have h_char : Ideal.span {(N : ℤ)} ≤ P.comap (algebraMap ℤ Ok5557) := by
                    exact hP.1.le;
                  exact Ideal.Quotient.eq_zero_iff_mem.mpr ( h_char <| Ideal.mem_span_singleton_self _ );
                have h_char : ringChar (Ok5557 ⧸ P) = ringChar (ℤ ⧸ Ideal.span {(N : ℤ)}) := by
                  have h_char_eq : ∀ (n : ℕ), (Ideal.Quotient.mk P) ((algebraMap ℤ Ok5557) n) = 0 ↔ (Ideal.Quotient.mk (Ideal.span {(N : ℤ)})) n = 0 := by
                    intro n; rw [ Ideal.Quotient.eq_zero_iff_mem ] ; rw [ Ideal.Quotient.eq_zero_iff_mem ] ; simp +decide [ Ideal.mem_span_singleton ] ;
                    have := hP.1; simp_all +decide [ Ideal.mem_span_singleton ] ;
                    rw [ SetLike.ext_iff ] at this; specialize this n; simp_all +decide [ Ideal.mem_span_singleton ] ;
                  rw [ ringChar.eq_iff ];
                  constructor;
                  intro n; rw [ ← ringChar.spec ] ;
                  convert h_char_eq n using 1;
                exact h_char;
              rw [ h_char ];
              rw [ ringChar.eq_iff ];
              constructor;
              intro x; erw [ Ideal.Quotient.eq_zero_iff_mem ] ; erw [ Ideal.mem_span_singleton ] ; norm_cast;
            have h_frobenius : ∀ (a b : Ok5557 ⧸ P), (a + b) ^ ringChar (Ok5557 ⧸ P) = a ^ ringChar (Ok5557 ⧸ P) + b ^ ringChar (Ok5557 ⧸ P) := by
              haveI := Fact.mk ( show Nat.Prime ( ringChar ( Ok5557 ⧸ P ) ) from h_char.symm ▸ Fact.out ) ; simp +decide [ add_pow_char ] ;
            rw [ ← h_char, h_frobenius ];
          exact h_frobenius;
        convert h_frobenius _ _ using 1;
        convert congr_arg₂ ( · + · ) ihx ihy using 1;
        rw [ map_add ];
        exact?;
        exact h_frobenius _ _;
        convert h_frobenius _ _ using 1;
        exact?;
      · simp_all +decide [ mul_pow, map_mul ];
    have := @Ok5557_eq_adjoin;
    exact h_adjoin x <| this ▸ Algebra.mem_top

/-
Define the cubic character and prove it is multiplicative.
-/
def chi5557 (x : ZMod 5557) : ZMod 5557 := x ^ 1852

lemma chi5557_mul (x y : ZMod 5557) : chi5557 (x * y) = chi5557 x * chi5557 y := by
  by_cases hx : x = 0 <;> by_cases hy : y = 0 <;> simp +decide [ hx, hy, chi5557 ];
  rw [ mul_pow ]

/-
Check the value of chi(g).
-/
def check_chi_values_5557 : IO Unit := do
  let q := 5557
  let s1 := 1827
  let s2 := 3729
  let g := 5
  let chi (n : Nat) := (n : Nat) ^ 1852 % q
  let chig := chi g
  IO.println s!"chi(g) = {chig}"
  if chig == s1 then IO.println "chi(g) = s1"
  else if chig == s2 then IO.println "chi(g) = s2"
  else IO.println "chi(g) is neither"

#eval check_chi_values_5557

/-
Prove the action of sigma on the periods.
-/
lemma sigma_eta_5557 (n : (ZMod 5557)ˣ) (S : Finset (ZMod 5557)) :
  sigma_int5557 n (eta_int5557 S) = eta_int5557 (S.image (fun x => (n : ZMod 5557) * x)) := by
    unfold eta_int5557;
    rw [ Finset.sum_image, map_sum ];
    · apply Finset.sum_congr rfl
      intro x hx
      simp [sigma_int_zeta_pow_5557];
      rw [ ← pow_mul, ZMod.val_mul ];
      rw [ ← Nat.mod_add_div ( ( n : ZMod 5557 ).val * x.val ) 5557, pow_add, pow_mul ] ; norm_num;
      erw [ show zeta_int5557 ^ 5557 = 1 from ?_ ] ; norm_num;
      exact zeta_int_isPrimitiveRoot_5557.pow_eq_one;
    · exact fun x hx y hy hxy => mul_left_cancel₀ ( Units.ne_zero n ) hxy

/-
Prove that multiplication by an element of H permutes the sets H, C1, C2 trivially.
-/
lemma action_H_5557 (n : ZMod 5557) (hn : n ∈ H5557) :
  H5557.image (fun x => n * x) = H5557 ∧
  C1_5557.image (fun x => n * x) = C1_5557 ∧
  C2_5557.image (fun x => n * x) = C2_5557 := by
    -- Since $n \in H5557$, we can write $n = x^3$ for some $x \in (\mathbb{Z}/5557\mathbb{Z})^\times$.
    obtain ⟨x, hx⟩ : ∃ x : ZMod 5557, x ≠ 0 ∧ n = x^3 := by
      unfold H5557 at hn; aesop;
    refine' ⟨ _, _, _ ⟩ <;> ext y <;> simp +decide [ *, H5557, C1_5557, C2_5557 ];
    · constructor <;> rintro ⟨ a, ha, rfl ⟩;
      · exact ⟨ x * a, mul_ne_zero hx.1 ha, by ring ⟩;
      · -- Since $x \neq 0$, we can find $a_1$ such that $x * a_1 = a$.
        obtain ⟨a_1, ha_1⟩ : ∃ a_1 : ZMod 5557, x * a_1 = a := by
          haveI := Fact.mk ( by norm_num : Nat.Prime 5557 ) ; exact ⟨ a / x, mul_div_cancel₀ _ hx.1 ⟩ ;
        exact ⟨ a_1, by aesop_cat, by rw [ ← mul_pow, ha_1 ] ⟩;
    · constructor <;> rintro ⟨ a, ha, rfl ⟩;
      · use x * a;
        grind;
      · -- Since $x \neq 0$, we can divide both sides by $x^3$ to get $a_1^3 = a^3 / x^3$.
        obtain ⟨a_1, ha_1⟩ : ∃ a_1 : ZMod 5557, a_1^3 = a^3 / x^3 := by
          have h_inv : ∃ y : ZMod 5557, y^3 = x^3 := by
            use x;
          obtain ⟨ y, hy ⟩ := h_inv;
          use a * y⁻¹;
          rw [ mul_pow, ← hy ] ; ring;
        grind;
    · constructor <;> rintro ⟨ a, ha, rfl ⟩;
      · use x * a;
        grind;
      · -- Let $a_1 = x^{-1} * a$.
        use x⁻¹ * a;
        haveI := Fact.mk ( by native_decide : Nat.Prime 5557 ) ; simp +decide [ *, mul_assoc, mul_comm, mul_left_comm, pow_succ ] ;

/-
Prove that multiplication by an element of C1 permutes the sets H, C1, C2 cyclically.
-/
lemma action_C1_5557 (n : ZMod 5557) (hn : n ∈ C1_5557) :
  H5557.image (fun x => n * x) = C1_5557 ∧
  C1_5557.image (fun x => n * x) = C2_5557 ∧
  C2_5557.image (fun x => n * x) = H5557 := by
    -- Since $n \in C1_5557$, we can write $n = 5 * h$ for some $h \in H5557$.
    obtain ⟨h, hh⟩ : ∃ h ∈ H5557, n = 5 * h := by
      native_decide +revert;
    -- Since $h \in H5557$, multiplying by $h$ permutes $H5557$, $C1_5557$, and $C2_5557$ trivially.
    have h_perm : H5557.image (fun x => h * x) = H5557 ∧ C1_5557.image (fun x => h * x) = C1_5557 ∧ C2_5557.image (fun x => h * x) = C2_5557 := by
      exact action_H_5557 h hh.1;
    simp_all +decide [ mul_assoc, Finset.image_image ];
    have h_perm : Finset.image (fun x => 5 * x) H5557 = C1_5557 ∧ Finset.image (fun x => 5 * x) C1_5557 = C2_5557 ∧ Finset.image (fun x => 5 * x) C2_5557 = H5557 := by
      native_decide +revert;
    have h_perm : Finset.image (fun x => 5 * (h * x)) H5557 = Finset.image (fun x => 5 * x) (Finset.image (fun x => h * x) H5557) ∧ Finset.image (fun x => 5 * (h * x)) C1_5557 = Finset.image (fun x => 5 * x) (Finset.image (fun x => h * x) C1_5557) ∧ Finset.image (fun x => 5 * (h * x)) C2_5557 = Finset.image (fun x => 5 * x) (Finset.image (fun x => h * x) C2_5557) := by
      simp +decide [ Finset.ext_iff ];
    aesop

/-
Prove that multiplication by an element of C2 permutes the sets H, C1, C2 cyclically.
-/
lemma action_C2_5557 (n : ZMod 5557) (hn : n ∈ C2_5557) :
  H5557.image (fun x => n * x) = C2_5557 ∧
  C1_5557.image (fun x => n * x) = H5557 ∧
  C2_5557.image (fun x => n * x) = C1_5557 := by
    revert hn n;
    -- By definition of $C2_5557$, we know that $n \in C2_5557$ implies $n = g^2 * h$ for some $h \in H_5557$.
    intro n hn
    obtain ⟨h, hh⟩ : ∃ h ∈ H5557, n = g5557^2 * h := by
      native_decide +revert;
    have h_perm : Finset.image (fun x => g5557^2 * x) H5557 = C2_5557 ∧ Finset.image (fun x => g5557^2 * x) C1_5557 = H5557 ∧ Finset.image (fun x => g5557^2 * x) C2_5557 = C1_5557 := by
      native_decide +revert;
    have h_perm : Finset.image (fun x => h * x) H5557 = H5557 ∧ Finset.image (fun x => h * x) C1_5557 = C1_5557 ∧ Finset.image (fun x => h * x) C2_5557 = C2_5557 := by
      exact action_H_5557 h hh.1;
    have h_perm : Finset.image (fun x => g5557^2 * h * x) H5557 = Finset.image (fun x => g5557^2 * x) (Finset.image (fun x => h * x) H5557) ∧ Finset.image (fun x => g5557^2 * h * x) C1_5557 = Finset.image (fun x => g5557^2 * x) (Finset.image (fun x => h * x) C1_5557) ∧ Finset.image (fun x => g5557^2 * h * x) C2_5557 = Finset.image (fun x => g5557^2 * x) (Finset.image (fun x => h * x) C2_5557) := by
      simp +decide [ mul_assoc, Finset.ext_iff ];
    aesop

/-
Prove the permutation of the periods under the Galois action.
-/
lemma global_permutation_5557 (n : (ZMod 5557)ˣ) :
  ((n : ZMod 5557) ∈ C1_5557 → sigma_int5557 n alpha_int5557 = beta_int5557) ∧
  ((n : ZMod 5557) ∈ C2_5557 → sigma_int5557 n alpha_int5557 = gamma_int5557) ∧
  ((n : ZMod 5557) ∈ H5557 → sigma_int5557 n alpha_int5557 = alpha_int5557) := by
    refine' ⟨ _, _, _ ⟩;
    · intro hn;
      unfold alpha_int5557 beta_int5557;
      unfold alpha_prime_int5557 beta_prime_int5557; simp +decide [ mul_assoc, map_mul, map_sub ] ;
      unfold eta0_int5557 eta1_int5557 eta2_int5557; simp +decide [ sigma_eta_5557, action_C1_5557 _ hn ] ;
    · unfold alpha_int5557 gamma_int5557;
      unfold alpha_prime_int5557 gamma_prime_int5557; simp +decide [ mul_sub ] ;
      unfold eta0_int5557 eta1_int5557 eta2_int5557; simp +decide [ ← mul_sub, ← map_sub, ← map_mul ] ;
      intro hn; rw [ map_sub ] ; rw [ sigma_eta_5557, sigma_eta_5557 ] ;
      rw [ action_C2_5557 _ hn |>.1, action_C2_5557 _ hn |>.2.1 ] ; norm_num;
    · intro hn;
      -- By definition of $alpha_int5557$, we have $alpha_int5557 = sign_5557 * alpha_prime_int5557$.
      simp [alpha_int5557, alpha_prime_int5557];
      -- By definition of $eta0_int5557$ and $eta1_int5557$, we know that
      simp [eta0_int5557, eta1_int5557];
      rw [ sigma_eta_5557, sigma_eta_5557 ];
      rw [ action_H_5557 _ hn |>.1, action_H_5557 _ hn |>.2.1 ] ; norm_num

