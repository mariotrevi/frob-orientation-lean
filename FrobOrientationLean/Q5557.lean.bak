def s2_5557 : ZMod 5557 := 3729
def c5557 (N : ℕ) : ZMod 5557 := (N : ZMod 5557)^1852

/-
Find a generator and define the field.
-/
def find_g_5557 : IO Unit := do
  let q := 5557
  let s2 := 3729
  let c (n : Nat) := (n : Nat) ^ 1852 % q
  let order := q - 1
  let factors := [2, 3, 463] -- 5556 = 2^2 * 3 * 463
  
  for g in [2:100] do
    let mut is_prim := true
    for f in factors do
      if (g ^ (order / f)) % q == 1 then
        is_prim := false
        break
    
    if is_prim then
      if c g == s2 then
        IO.println s!"Found good generator (c(g)=s2): {g}"
        return
      else
        IO.println s!"Found generator (c(g)!=s2): {g}"

  IO.println "No generator found in range"

