  (chi10513 N = s1_10513 → alpha_bar ^ N = gamma_bar) ∧
  (chi10513 N = s2_10513 → alpha_bar ^ N = beta_bar) := by
    obtain ⟨h₁, h₂⟩ := h_assum;
    grind