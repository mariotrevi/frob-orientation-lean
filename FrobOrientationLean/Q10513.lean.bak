    convert congr_arg ( Ideal.Quotient.mk P ) h_root_of_unity using 1;
    convert congr_arg ( Ideal.Quotient.mk P ) ( sigma_int_zeta_pow_1567 ( N_mod_1567 N hN ) ) using 1

