theorem Ok10513_eq_adjoin : (⊤ : Subalgebra ℤ Ok10513) = Algebra.adjoin ℤ {zeta_int10513} := by
  have h_cyclotomic_int : IsCyclotomicExtension {10513} ℤ Ok10513 := by
