      · rw [ Finset.card_image_of_injective _ fun x y hxy => mul_left_cancel₀ ( show n ≠ 0 from by rintro rfl; exact absurd hn ( by native_decide ) ) hxy ];
    · refine' Finset.eq_of_subset_of_card_le _ _;
