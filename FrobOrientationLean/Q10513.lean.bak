theorem final_result_10513 (N : ℕ) [Fact (Nat.Prime N)] (hN : N ≠ 10513) (hN_odd : N % 2 = 1) (hN_a : Nat.Coprime 205 N)
  (P : Ideal Ok10513) [P.IsMaximal] (hP : P.LiesOver (Ideal.span {(N : ℤ)}))
